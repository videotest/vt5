[Script]
Form.Title = Docs.LoadString ("Table Options")

Dim bHorz 

Sub Shift ()
	Select Case bHorz
		Case 0
			Horizont.SetPressedState
			Vertical.ResetPressedState
			ByColumn.ResetPressedState
		Case 1
			Horizont.ResetPressedState
			Vertical.SetPressedState
			ByColumn.ResetPressedState		
		Case 2
			Horizont.ResetPressedState
			Vertical.ResetPressedState
			ByColumn.SetPressedState
	End Select

End Sub
'***************************************************************
'Horizont event handlers.
'***************************************************************
Sub Horizont_Click()
	bHorz =0
	Shift
End Sub

'***************************************************************
'Vertical event handlers.
'***************************************************************
Sub Vertical_Click()
	bHorz =1
	Shift
End Sub

'***************************************************************
'Form event handlers.
'***************************************************************
Sub Form_OnOK()
	Data.SetValue "\ManualMeasure\ManualMeasShiftMode", bHorz
	Form.UpdateSystemSettings
End Sub

Sub Form_OnInitialize()
	frmShift.Text = Docs.LoadString ("Colors:")
	Horizont.ButtonText = Docs.LoadString ("Horizontal")
	Vertical.ButtonText = Docs.LoadString ("Vertical")
	ByColumn.ButtonText = Docs.LoadString ("By Column")
	Colors.SetColumnCaption 0, Docs.LoadString ("Type")
	Colors.SetColumnCaption 1, Docs.LoadString ("Color")
	Colors.SetCellValue 0, 0, Docs.LoadString ("Linear")
	Colors.SetCellValue 0, 1, Docs.LoadString ("Angle")
	Colors.SetCellValue 0, 2, Docs.LoadString ("Counter")
	Colors.SetCellValue 0, 3, Docs.LoadString ("Undefined")
	Colors.SetCellValue 0, 4, Docs.LoadString ("Background")

	bHorz = Data.Getvalue( "\ManualMeasure\ManualMeasShiftMode" )
	Shift
End Sub
'***************************************************************
'ByColumn event handlers.
'***************************************************************
Sub ByColumn_Click()
	bHorz =2
	Shift
End Sub