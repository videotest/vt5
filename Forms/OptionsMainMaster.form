[Script]
Dim StartGamma, StartBits
Dim global 
Dim bNeedActivateBinary
bNeedActivateBinary = false
Set global = Application.GetglobalScript("")
'***************************************************************
'Form event handlers.
'***************************************************************
Sub Form_OnInitialize()
	Form.Title = Docs.LoadString ("Main Options")
	chkNewImg.Caption = Docs.LoadString ("New element after action")
	chkModified.Caption = Docs.LoadString ("Ask for save modified")
	edtUndo.CaptionText = Docs.LoadString ("Undo Number:")
	edtGamma.CaptionText = Docs.LoadString ("Gamma:")
	edtHiBitNum.CaptionText = Docs.LoadString ("Show Bits:")
	frmDisplay.Text = Docs.LoadString ("Display")
	btnDefault.ButtonText = Docs.LoadString ("Default")
	Colors.SetColumnCaption 0, Docs.LoadString ("Name")
	Colors.SetColumnCaption 1, Docs.LoadString ("Color")
	Colors.SetCellValue 0, 0, Docs.LoadString ("Selection")
	Colors.SetCellValue 0, 1, Docs.LoadString ("Background")
	Colors.SetCellValue 0, 2, Docs.LoadString ("Masks")
	
	If Data.Getvalue ("\General\PromtCloseModified") Then chkModified.Checked 1
	If Data.Getvalue ("\General\DeleteFilterArgs")=0 Then chkNewImg.Checked 1
	edtGamma.ValueDouble = CLng( Data.GetDefValue ("\DrawImage\Gamma", 1) )
	edtUndo.ValueInt = Data.GetDefValue ("\ActionManager\UndoListMaxSize", 15)
	StartGamma = edtGamma.ValueDouble
	sldGamma.PosLo = edtGamma.ValueDouble * 100
	edtHiBitNum.ValueInt = Data.GetDefValue ("\DrawImage\HiBitNum", 16)
	StartBits = edtHiBitNum.ValueInt
End Sub

Sub Form_OnOK()
	Data.Setvalue "\General\DeleteFilterArgs", 1-Abs(chkNewImg.Checked)
	Data.Setvalue "\DrawImage\HiBitNum", edtHiBitNum.ValueInt
	Data.Setvalue "\DrawImage\Gamma", edtGamma.ValueDouble
	Data.Setvalue "\ActionManager\UndoListMaxSize", edtUndo.ValueInt	
	Data.Setvalue "\General\PromtCloseModified", Abs(chkModified.Checked)
	Form.UpdateSystemSettings
	Data.SetValue "\Binary\BackColor", Data.GetValue ("\Colors\Background")
	Data.SetValue "\Image\Background", Data.GetValue ("\Colors\Background")
	Form.UpdateSystemSettings
	If bNeedActivateBinary and global.GetCurrentView = "ImageDoc.BinaryFore" Then
		Freeze 1
		ShowSource
		ShowBinaryFore
		Freeze 0
	End If
End Sub

Sub Form_OnCancel()
	edtGamma.ValueDouble = StartGamma 
	edtHiBitNum.ValueInt = StartBits
	Data.Setvalue "\DrawImage\HiBitNum", edtHiBitNum.ValueInt
	Data.Setvalue "\DrawImage\Gamma", edtGamma.ValueDouble
	Form.UpdateSystemSettings
End Sub

'***************************************************************
'sldGamma event handlers.
'***************************************************************
Sub sldGamma_PosChanged()
	edtGamma.ValueDouble = sldGamma.PosLo/100
	Data.Setvalue "\DrawImage\Gamma", edtGamma.ValueDouble
	Form.UpdateSystemSettings
End Sub
'***************************************************************
'btnDefault event handlers.
'***************************************************************
Sub btnDefault_Click()
	edtGamma.ValueDouble = 1
	sldGamma.PosLo = edtGamma.ValueDouble * 100
	Data.Setvalue "\DrawImage\Gamma", edtGamma.ValueDouble
	edtHiBitNum.ValueInt = 16
	Data.Setvalue "\DrawImage\HiBitNum", edtHiBitNum.ValueInt
	Form.UpdateSystemSettings
End Sub
'***************************************************************
'edtHiBitNum event handlers.
'***************************************************************
Sub edtHiBitNum_DataChange()
	Data.Setvalue "\DrawImage\HiBitNum", edtHiBitNum.ValueInt
	Form.UpdateSystemSettings
End Sub

'***************************************************************
'edtGamma event handlers.
'***************************************************************
Sub edtGamma_DataChange()
	sldGamma.PosLo = edtGamma.ValueDouble * 100
End Sub

sub Form_OnDestroy()
	Set global = Nothing
end sub

'***************************************************************
'Colors event handlers.
'***************************************************************
sub Colors_DataChange(nColumn, nRow)
	If nColumn = 1 and nRow = 2 Then bNeedActivateBinary = true
end sub

