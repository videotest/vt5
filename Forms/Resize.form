[Script]
Option Explicit

Form.Title = Docs.LoadString ("Resize")

Dim CoeffXY, rec, img, sVerySmallImg, sInformation

Set img = Objects.ActiveImage
On Error Resume Next

'***************************************************************
'Form event handlers.
'***************************************************************
Sub Form_OnDestroy()
	img = Null
End Sub
Sub Form_OnOK()
	Data.Setvalue "\Resize\Prop", chkProp.Checked 
	Data.Setvalue "\Resize\X", CDbl(edtX.ValueInt)
	Data.Setvalue "\Resize\Y", CDbl(edtY.ValueInt)
	If CDbl(edtX.ValueInt) > 0 And CDbl(edtY.ValueInt )> 0 Then 
		If edtX.ValueInt <> txtX.Text Or edtY.ValueInt <> txtY.Text Then ActionManager.Resize edtX.ValueInt, edtY.ValueInt
	Else
		MsgBox sVerySmallImg,vbOkOnly+vbInformation,sInformation
	End If
End Sub
Sub Form_OnInitialize()	
	txtSourceImage.Text = Docs.LoadString ("Source Image")
	txtNewImage.Text = Docs.LoadString ("New Image")
	txtWidth1.Text = Docs.LoadString ("Width")
	txtHeight1.Text = Docs.LoadString ("Height")
	edtX.CaptionText = Docs.LoadString ("Width")
	edtY.CaptionText = Docs.LoadString ("Height")
	txtRario.Text = Docs.LoadString ("      X/Y ratio")
	chkProp.Caption = Docs.LoadString ("Save ratio")
	sVerySmallImg = Docs.LoadString ("The image is too small!")
	sInformation = Docs.LoadString ("Information")

	rec = 0
	edtX.ValueInt = CDbl(img.GetWidth)
	edtY.ValueInt = CDbl(img.GetHeight)
	txtX.Text = edtX.ValueInt
	txtY.Text = edtY.ValueInt
	If img.GetWidth Then CoeffXY = img.GetWidth / img.GetHeight
	txtXY.Text = FormatNumber( CoeffXY ,2 )  
	If Data.Getvalue ("\Resize\Prop") Then chkProp.Checked 1
End Sub
'***************************************************************
'edtX event handlers.
'***************************************************************
Sub edtX_DataChange()	
	If rec Then Exit Sub
	rec = 1
	If edtX.ValueInt <> "" Then
		If chkProp.Checked Then 
			edtY.ValueInt = CInt(edtX.ValueInt / CoeffXY )
		End If
	End If
	rec = 0
End Sub
'***************************************************************
'edtY event handlers.
'***************************************************************
Sub edtY_DataChange()
	If rec Then Exit Sub
	rec = 1
	If edtY.ValueInt <> "" Then
		If chkProp.Checked Then 
			edtX.ValueInt = CInt(edtY.ValueInt * CoeffXY )
		End If
	End If
	rec = 0
End Sub
'***************************************************************
'chkProp event handlers.
'***************************************************************
Sub chkProp_OnCheck(bCheck)
	If chkProp.Checked Then
		edtY.ValueInt = CInt(edtX.ValueInt / CoeffXY)
	End If
End Sub

