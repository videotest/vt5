' Удаление всех изм. объектов кроме активного //
Sub ManualMeasure_BeforeManualMeasure(StrType)
	Dim objList, iObjCount, i, o
	Set objList = Objects.ActiveObjectList
	If TypeName(objList) = "Nothing" Then Exit Sub
	iObjCount = objList.GetCount
	For i = iObjCount - 2 To 0 Step -1
		objList.RemoveObjectByIdx( i )
	Next
	Set objList = Nothing
End Sub

Sub ManualMeasure_AfterManualMeasure(StrType, result)
	MeasReasult = Data.Getvalue ("\ManualMeasure\LastPixelLength") 
	
	FormManager.EnableOk False
	
	If MeasReasult = 0 Then	Exit Sub
	
	txtName.Disabled = False	
	txtResult.Text = FormatNumber(MeasReasult,2)
	txtUnit.Text = sPixel
	edtDistance.Disabled = 0
	Msg.TextColor = &h000000 '&hFF0000
	Msg.Text = sMsg1
	'edtDistance.EditBackColor = &hFF0000	&hFF9428
	'edtDistance.EditTextColor = &hFFFFFF	&hFFFFFF
	edtDistance.Text = ""
	edtDistance.SetFocus
	'CalibrList.SetColumnBackColor 0,&hFFFFFF
End Sub

Sub LoadUnits()	
	iCountUnit = Data.Getvalue ("\UnitList\CountUnit")
	iCurrentUnit = Data.Getvalue ("\UnitList\CurrentUnit")
	ReDim  szUnit(iCountUnit )
	ReDim  fUnitCoeff(iCountUnit )
	For i=0 To iCountUnit-1
		Name = "\UnitList\C0R"+CStr(i)
		s = Data.Getvalue (Name)
		cbUnit.InsertString i,s
		Name = "\UnitList\C1R"+CStr(i)
		fUnitCoeff(i) = Application.VTDbl( Data.Getvalue (Name) )
	Next
	cbUnit.SetSel iCurrentUnit
	UnitCoeff = fUnitCoeff(iCurrentUnit)
	Unit = cbUnit.GetText()
End Sub

'***************************************************************
'Form event handlers.
'***************************************************************
Dim obj_list
bWasOk = False

Sub Form_OnInitialize()
	Set obj_list = Objects.ActiveObjectList
	FirstCall = 1
	'ActionManager.HideDockBars

	'Активизация временного списка объектов без изменения вью //
	ActionManager.ObjectNew "Object List", "_TmpResults1", 0
	ActionManager.ObjectSelect "_TmpResults1", 2

	Set CalibrObj = Objects.GetObjectByName( "_TmpResults1" )
	Objects.ActiveObjectList = CalibrObj	
	LoadUnits()
	
	sSec = "\Input\" & Data.GetDefValue ("\Input\Device", "Open File") & "\PixelAspectRatio"
	dXYRatio = Data.GetDefValue (sSec, 1)

	ActionManager.ShowImage

	EditBackColor = edtDistance.EditBackColor
	Msg.Text = sMsg2
	edtDistance.Disabled = 1
	'cbUnit.Disabled = 1
	'MsgBox "Measure test object!"
	NameAction = ActionState.GetCurrentAction

	'3 строки перенести в Sub Form_OnShow()

	'Блокируем контекстное меню для предотвращения завершения акции MeasManualLine //
	ActionState.SetActionState "ShowSource", 0
	ActionState.SetActionState "ShowImage", 0
	ActionState.SetActionState "ShowMasks", 0
	ActionState.SetActionState "ShowObjects", 0
	ActionState.SetActionState "ShowBinaryFore", 0
	ActionState.SetActionState "ShowBinaryBack", 0
	ActionState.SetActionState "ShowMeasView", 0
	ActionState.SetActionState "ShowStatView", 0
	ActionState.SetActionState "ShowStatTable", 0
	ActionState.SetActionState "ShowPropertyBar", 0
	ActionState.SetActionState "SendTo", 0
	
	MakeNew = 1	
	FirstCall = 0
	txtUnit.Text = sPixel

	txtName.Text = NewName()
	txtName.Disabled = True
'	txtName.Disabled = false
	
	FormManager.EnableOk False
	FormManager.EnableApply False
	
	If NameAction <> "MeasManualLine" Then
'		ActionManager.MeasManualLine
		ActionState.RunInteractive "MeasManualLine"
	End If
	Data.RemoveValue "\tmp"
	txtMsg.Text = ""	
End Sub

Sub Form_OnOK()
	If cbUnit.GetSel() < 0 Then cbUnit.SetSel(iCurrentUnit)	
	If txtName.Text = "" or Coeff = 0 Then Exit Sub
	If MeasReasult = 0 Then Exit Sub
	Data.Setvalue "\UnitList\CurrentUnit", cbUnit.GetSel()		
	Data.Setvalue "\Units\Linear\Name", cbUnit.GetText()
	Data.Setvalue "\Units\Linear\CoeffToUnit", fUnitCoeff(cbUnit.GetSel())
	Data.Setvalue "\Calibration\MeasPix", 0
	
	Data.Setvalue "\Units\Velocity\Name", cbUnit.GetText() + Docs.LoadString ("/s")
	Data.Setvalue "\Units\Velocity\CoeffToUnit", fUnitCoeff(cbUnit.GetSel())	

	CalibrGUID = Docs.GenerateGUID
	Data.Setvalue "\"+sSection+"\"+"Active", CalibrGUID
	Data.Setvalue "\"+sSection+"\"+CalibrGUID+"\"+"Name", txtName.Text
	Data.Setvalue "\"+sSection+"\"+CalibrGUID+"\"+"MeterPerPixel", Coeff/UnitCoeff
	Data.Setvalue "\"+sSection+"\"+CalibrGUID+"\"+"Active", 1

	ActionManager.ReCalcOnChangeCalibr
	Form.UpdateSystemSettings
	bWasOk = True
	Data.SetValue "\tmp\FormOK", 1
End Sub

Sub Form_OnDestroy()
	bFormClose = True
	'ActionManager.ShowDockBars 
	ActionState.TerminateCurrentAction
	Objects.RemoveObject CalibrObj 
	Set CalibrObj = Nothing
	If TypeName(obj_list) <> "Nothing" Then Objects.ActiveObjectList = obj_list
	Set obj_list = Nothing
	If bWasOk Then ActionManager.CalcMeasResult

	ActionState.ResetActionState "ShowSource"
	ActionState.ResetActionState "ShowImage"
	ActionState.ResetActionState "ShowMasks"
	ActionState.ResetActionState "ShowObjects"
	ActionState.ResetActionState "ShowBinaryFore"
	ActionState.ResetActionState "ShowBinaryBack"
	ActionState.ResetActionState "ShowMeasView"
	ActionState.ResetActionState "ShowStatView"
	ActionState.ResetActionState "ShowStatTable"
	ActionState.ResetActionState "ShowPropertyBar"
	ActionState.ResetActionState "SendTo"
End Sub


'***************************************************************
'edtDistance event handlers.
'***************************************************************
Sub edtDistance_DataChange()
	If 	edtDistance.ValueDouble > 0 and MeasReasult > 0 Then 		
		Coeff = edtDistance.ValueDouble / MeasReasult 
		txtCoeff.Text = FormatNumber(Coeff,6)+" "+Unit+"/"+sPixel
		FormManager.EnableOk True
	Else
		FormManager.EnableOk False
	End If
End Sub

'***************************************************************
'txtName event handlers.
'***************************************************************
sub txtName_DataChange()
	FormManager.EnableOk( Trim(txtName.Text) <> "" And Not NameExist(txtName.Text))
	If NameExist(txtName.Text) Then 
		txtMsg.Text Docs.LoadString ("The specified name already exists!")
	Else
		txtMsg.Text ""
	End If
end sub

Sub Show(from)
	Stroka = ""
	Stroka = Stroka+"N="+CStr(CalibrCount)+" "+" From "+from+"  Current "+CStr(Current)+vbCrLf
	For i=0 To CalibrCount-1 
		Stroka = Stroka + CStr(i)+" "+CalibrName (i)+ " "+CStr(CalibrCoeff(i))+" "+CStr(CalibrGUID (i))+vbCrLf
	Next
	Stroka = Stroka + CStr(CalibrList.GetRowCount)

	Stroka = Stroka+vbCrLf
	For i=0 To CalibrCount-1 
		Stroka = Stroka + CStr(i)+" "+CalibrList.GetCellAsString(0,i)+vbCrLf
	Next
End Sub
'***************************************************************
'cbUnit event handlers.
'***************************************************************
Sub cbUnit_OnChange()
	cbUnit.SetSel(iCurrentUnit)	
End Sub
Sub cbUnit_OnSelectChange()
	If FirstCall=0 Then 
		Unit = cbUnit.GetText()
		UnitCoeff = fUnitCoeff(cbUnit.GetSel())
		txtCoeff.Text = FormatNumber(Coeff,6)+" "+Unit+"/"+sPixel
		iCurrentUnit = cbUnit.GetSel()
	End If
End Sub

'==========================================================
' Отлавливаем завершение акций //
'==========================================================
Sub ActionState_OnActionComplete(strActionName, intActionResult)
	If strActionName = "MeasManualLine" And intActionResult = 0 And Not bFormClose Then
'	  MsgBox "MeasManualLine: " & intActionResult
	  ActionState.RunInteractive "MeasManualLine"
	End If
End Sub

Function NameExist(str)
	NameExist = false
	Data.SetSection +"\"+sSection
	a = Data.GetEntriesCount 
	For i=0 To a-1
		sStr = Data.GetEntryName (i)
		If Left (sStr,1) = "{" Then 
			sName = Data.Getvalue ("\"+sSection+"\"+sStr+"\"+"Name")
			If str = sName Then
				NameExist = true
				Exit Function
			End If 
		End If 
	Next		
End Function

Function NewName()
	Dim i,str
	i = 0
	str = sNewCalibr
	While NameExist(str)
		i = i + 1
		str = sNewCalibr + CStr(i)
	Wend
	NewName = str
End Function

