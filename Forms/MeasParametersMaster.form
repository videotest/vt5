'-------------------------------------------------------
' Заполнение списка групп (левого) //

Sub lstGroupsFill
	intDiff = numGroup - lstGroups.GetRowCount
	If intDiff > 0 Then
		For i = 1 To intDiff
			lstGroups.AddRow(0)
		Next
	ElseIf intDiff < 0 Then
		For i = 1 To -intDiff
			lstGroups.DeleteRow(0)
		Next
	End If
	For i = 1 To numGroup 
		lstGroups.SetCellValue 1, i-1, arrParams(i,0,P_NAME)
	Next
End Sub


'***************************************************************
'lstGroups event handlers.
'***************************************************************
Sub lstGroups_CurentRowChange(nRow)
	Dim bGroupChanged
	bGroupChanged = True
	If activeGroup = (nRow + 1) Then bGroupChanged = False
	
	activeGroup = nRow + 1
	numParam = arrParams(activeGroup,0,P_NUMBER)

	If bGroupChanged Then
		intDiff = numParam - lstParams.GetRowCount
		If intDiff > 0 Then
			For i = 1 To intDiff
				lstParams.AddRow(0)
			Next
		ElseIf intDiff < 0 Then
			For i = 1 To -intDiff
				lstParams.DeleteRow(0)
			Next
		End If
	End If

	For i = 1 To numParam
		intCheck = CInt(arrParams(activeGroup,i,P_CHECKED))
		lstParams.SetCellAsLogical 0, i-1, intCheck
		txtGroupTitle = Data.GetValue(txtMPSection & "\" & arrParams(activeGroup,i,P_NAME) & "\UserName")
		lstParams.SetCellValue 1, i-1, txtGroupTitle
		'lstParams.SetCellValue 2, i-1, UserNumFormat(arrParams(activeGroup,i,P_ACCURACY))
		lstParams.SetCellValue 2, i-1, arrParams(activeGroup,i,P_ACCURACY)
	Next

	If bGroupChanged Then
		lstParams.SetCurentRow(0)
		lstParams_CurentRowChange(0)
	End If
End Sub

Sub lstGroups_DataChange(intColumn,intRow)
	On Error Resume Next
	intGroupCheck = 0
	For i = 1 To numParam
		If arrParams(activeGroup, i, P_CHECKED) = 0 Then
			intGroupCheck = 1
			Exit For
		End If
	Next

	bFoundUnchecked = False
	For i = 1 To numParam
		If arrParams(activeGroup, i, P_CHECKED) <> 2 Then
			arrParams(activeGroup, i, P_CHECKED) = intGroupCheck
			lstParams.SetCellAsLogical 0, i-1, intGroupCheck
		Else
			bFoundUnchecked = True
		End If
		txtKey = arrParams(activeGroup, i, P_NAME)
		mp.Item(txtKey) = arrParams(activeGroup, i, P_CHECKED)
	Next
	If intGroupCheck = 0 And bFoundUnchecked Then
		lstGroups.SetCellAsLogical 0,intRow, 2
	Else
		lstGroups.SetCellAsLogical 0,intRow, intGroupCheck
	End If
End Sub

'***************************************************************
'lstParams event handlers.
'***************************************************************
Sub lstParams_DataChange(intColumn,intRow)
	isCheck = 0
	isUnCheck = 0
	For i = 1 To numParam
		If arrParams(activeGroup, i, P_CHECKED) = 2 Then
			lstParams.SetCellAsLogical 0,i-1,2
			intCheck = 2
		Else
			intCheck = lstParams.GetCellAsLogical(0,i-1)
		End If
		If intCheck = 0 Then
			isUnCheck = 1
		Else
			isCheck = 1
		End If
	Next
	lstGroups.SetCellAsLogical 0,activeGroup-1, isCheck + isUnCheck * isCheck
	intCheck = lstParams.GetCellAsLogical(0,intRow)
	arrParams(activeGroup, intRow+1, P_CHECKED) = intCheck
	txtKey = arrParams(activeGroup, intRow+1, P_NAME)
	mp.Item(txtKey) = intCheck
	'включение параметров из ф-лы, если включили юзерский параметр //
	If intCheck Then Call check_user_param( arrParams(activeGroup,activeParam,P_KEY) )
End Sub

Sub lstParams_CurentRowChange(nRow)
	If nRow < 0 Then Exit Sub
	activeParam = nRow + 1
	'fldAccuracy.Text = lstParams.GetCellAsString(2, activeParam - 1)
	edtAccuracy.ValueInt = lstParams.GetCellAsString(2, activeParam - 1)
End Sub

