[Script]
Const P_NAME = 0, P_TITLE = 1, P_CHECKED = 2, P_NUMBER = 2, P_ACCURACY = 3, P_KEY = 4, P_EXPRESSION = 5
Dim lenGroup, lenParam, rec
lenGroup = 20
lenParam = 40
Dim arrParams(), numGroup, activeGroup, activeParam, numParam, txtMPSection, txtGroupSection
ReDim arrParams(20,40,5)

Dim mp
Set mp = CreateObject("Scripting.Dictionary")

txtMPSection = "\measurement\parameters"
txtGroupSection = "\measurement\groups"

'***************************************************************
'Form event handlers.
'***************************************************************
Sub Form_OnInitialize()
	wdtWidth.max = 80
	edtArea.ValueInt = Data.Getvalue ("\measurement\MinArea")
	edtArea_DataChange()
	btnUserParam.ButtonText = Docs.LoadString ("Edit User Defined Parameters")
	btnSelectAll.ButtonText = Docs.LoadString ("Select All")
	btnClearAll.ButtonText = Docs.LoadString ("Clear All")
	lstGroups.SetColumnCaption 1, Docs.LoadString ("Group")
	lstParams.SetColumnCaption 1, Docs.LoadString ("Parameters")
	lstParams.SetColumnCaption 2, Docs.LoadString ("Dec. Places")
	fldAccuracyTitle.Text = Docs.LoadString ("Decimal places:")
	txtMinSize.Text = Docs.LoadString ("Minimum Size (pix)")
	edtArea.CaptionText = Docs.LoadString ("Area:")
	txtWidth.Text = Docs.LoadString ("Width:")
	sListMissing = Docs.LoadString ("The measurement parameters list file is missing or corrupt.")
	sReinstall = Docs.LoadString ("Reinstall VideoTesT to solve this problem.")
	sParedDown = Docs.LoadString ("Otherwise the set of parameters will be pared-down.")
	sAttention = Docs.LoadString ("Attention")

	Call scan_shelldata
	Call load_info
	Call update_new_groups
	activeGroup = -1	'чтобы на старте прорисовались параметры группы //
	lstGroups_CurentRowChange(0)
End Sub

Sub Form_OnOK()
	Call update_shell_data
	Form.UpdateSystemSettings
	Data.Setvalue "\measurement\MinArea",edtArea.ValueInt
'   k = Data.GetValue("\NewCalibr\" & Data.GetValue("\NewCalibr\Active") & "\MeterPerPixel")
'	Data.Setvalue "\Objects\_Small\0\lo", (3.14 * wdtWidth.Width * wdtWidth.Width/4) * k * k
'	Data.Setvalue "\Objects\_Small\0\hi",1e+300
	ActionManager.CalcMeasResult
End Sub

'***************************************************************
'Dictionary
'***************************************************************
Sub scan_shelldata
	Data.SetSection txtMPSection
	intCountParams = Data.GetEntriesCount
	For i = 0 To intCountParams - 1
		Data.SetSection txtMPSection
		txtParamName = Data.GetEntryName(i)
		mp.Add txtParamName, 2
	Next
End Sub

'***************************************************************
'File access
'***************************************************************
Sub load_info
	On Error Resume Next
	Call get_array
	Call lstGroupsFill
	Call set_groups_check
	
	activeGroup = 1							'по умолчанию активная //
	lstGroups.SetCurentRow activeGroup - 1	'группа - первая //
End Sub

'прорисовка чекбоксов для групп //
Sub set_groups_check
	For i = 1 To numGroup
		isCheck = 0
		isUnCheck = 0
		For j = 1 To arrParams(i,0,P_NUMBER)
			If arrParams(i,j,P_CHECKED) = 0 Then
				isUnCheck = 1
			Else
				isCheck = 1
			End If
		Next
		lstGroups.SetCellAsLogical 0,i-1, isCheck + isUnCheck * isCheck
	Next
End Sub

'-------------------------------------------------------
' Заполнение массива arrParams информацией из shell.data //

Sub get_array
	On Error Resume Next
	Dim param_name, param_checked, countGroup, countParam, sListMissing, sReinstall, sParedDown, sAttention
	countGroup = 1
	countParam = 0
	Data.SetSection txtGroupSection
	intCountGroups = Data.GetEntriesCount
	If intCountGroups = 0 Then
		Call create_default_list
		Data.SetSection txtGroupSection
		intCountGroups = Data.GetEntriesCount
	End If
	For i = 0 To intCountGroups - 1
		If countParam > 0 Then
			countGroup = countGroup + 1
		End If
		countParam = 0
		Data.SetSection txtGroupSection
		txtSubSection = Data.GetEntryName(i)
		arrParams(countGroup,0,P_NAME) = txtSubSection
		Data.SetSection txtGroupSection & "\" & txtSubSection
		intInnerCount = Data.GetEntriesCount
		For j = 0 To intInnerCount - 1
			Data.SetSection txtGroupSection & "\" & txtSubSection
			param_name = Data.GetEntryName(j)
			param_checked = Data.GetValue(txtGroupSection & "\" & txtSubSection & "\" & param_name)
			mpKey = param_name
			If mp.Exists(mpKey) Then
				If param_checked = 1 Then
					countParam = countParam + 1
					arrParams(countGroup,countParam,P_NAME) = param_name
					arrParams(countGroup,countParam,P_CHECKED) = Data.GetValue(txtMPSection & "\" & mpKey & "\Enable")
					If Data.GetDefValue(txtMPSection & "\" & mpKey & "\AlwaysCalc", 0) Then arrParams(countGroup,countParam,P_CHECKED) = 2
					arrParams(countGroup,countParam,P_KEY) = Data.GetDefValue(txtMPSection & "\" & mpKey & "\Key", -1)
					If Data.GetDefValue(txtMPSection & "\" & mpKey & "\Key", -1)>131072 Then
					arrParams(countGroup,countParam,P_EXPRESSION) = Data.GetDefValue(txtMPSection & "\" & mpKey & "\ExprString", "")
					End If
					arrParams(countGroup,countParam,P_ACCURACY) = Mid(Data.GetValue(txtMPSection & "\" & mpKey & "\Format"), 4, 1)
					mp.Item(mpKey) = Data.GetValue(txtMPSection & "\" & mpKey)
				Else
					mp.Item(mpKey) = 0
				End If
			End If
			arrParams(countGroup,0,P_NUMBER) = countParam
		Next
		'MsgBox countGroup
	Next
	If countParam = 0 Then countGroup = countGroup - 1
	numGroup = countGroup
End Sub

'-------------------------------------------------------
' Обновление shell.data по нажатию OK //

Sub update_shell_data
	Data.SetSection txtMPSection
	intCountParams = Data.GetEntriesCount
	For i = 0 To intCountParams - 1
		Data.SetSection txtMPSection
		txtParamName = Data.GetEntryName(i)
		Data.SetValue txtMPSection & "\" & txtParamName & "\Enable", 0
	Next
	For i = 1 To lenGroup
		For j = 1 To lenParam
			If arrParams(i,j,P_NAME) <> "" Then
				iEnable = arrParams(i,j,P_CHECKED)
				If iEnable > 1 Then iEnable = 1
				Data.SetValue txtMPSection & "\" & arrParams(i,j,P_NAME) & "\Enable", iEnable
				Data.SetValue txtMPSection & "\" & arrParams(i,j,P_NAME) & "\Format", "%0." & arrParams(i,j,P_ACCURACY) & "f"
			End If
		Next
	Next
End Sub

'-------------------------------------------------------
' Помещение найденных новых параметров в группу Extend и информирование об этом //

Sub update_new_groups
	wasFound = False
	mpKeys = mp.Keys
	mpItems = mp.Items
	For i = 0 To mp.Count - 1
		If mpItems(i) = 2 Then 
			Data.SetValue txtGroupSection & "\Extend\" & mpKeys(i), 0
			wasFound = True
		End If
	Next
'	If wasFound Then MsgBox "Found new parameters!", vbInformation, "For developers"
End Sub

'***************************************************************
'array operations
'***************************************************************
' Создание списка параметров по умолчанию //

Sub create_default_list
	MsgBox sListMissing & vbNewLine & _
		sReinstall & vbNewLine & _
		sParedDown, vbExclamation, sAttention
	Data.SetValue txtGroupSection & "\Area\Extern Area", 1
	Data.SetValue txtGroupSection & "\Area\Inner Area", 1
	Data.SetValue txtGroupSection & "\Area\Extern Perimeter", 1
	Data.SetValue txtGroupSection & "\Area\Inner Perimeter", 1

	Data.SetValue txtGroupSection & "\Sizes\Height", 1
	Data.SetValue txtGroupSection & "\Sizes\Width", 1
	Data.SetValue txtGroupSection & "\Sizes\Size by x", 1
	Data.SetValue txtGroupSection & "\Sizes\Size by y", 1
