sub edtArea_DataChange()
	s = edtArea.ValueInt
	If s>=2 Then
	d = int(sqr((4*s)/3.14)+.999999)
	rec = 1
	wdtWidth.Width = d
	rec = 0
	End If
end sub

'***************************************************************
'wdtWidth event handlers.
'***************************************************************
sub wdtWidth_OnChange()
	If rec Then Exit Sub
	rec = 1
	If wdtWidth.Width < 2 Then wdtWidth.Width = 2
	s = 3.14 * wdtWidth.Width * wdtWidth.Width/4
	If s>5000 Then s=5000
	If s>0 Then
		edtArea.ValueInt = cint(FormatNumber(s,0,0,0,0))
	Else
		edtArea.Text = "0"
	End If
	rec = 0
end sub
