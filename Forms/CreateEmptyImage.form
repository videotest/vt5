	cpkGRAYColor.SetColorRGB Data.GetDefValue ("\CreateImage\GRAYColor", "(0,0,0)")
	edtX.CaptionText = Docs.LoadString ("Width:")
	edtY.CaptionText = Docs.LoadString ("Height:")
	edtX.Text = Data.GetDefValue ("\CreateImage\X", "200") 
	edtY.Text = Data.GetDefValue ("\CreateImage\Y", "200") 
	
	If Data.Getvalue ("\CreateImage\GRAY") Then 
		rdbGRAY.ItemChecked = 0
		rdbRGB.ItemChecked = -1
		cpkGRAYColor.Enabled = True
		cpkRGBColor.Enabled = False
	Else 
		rdbGRAY.ItemChecked = -1
		rdbRGB.ItemChecked = 0
		cpkRGBColor.Enabled = True
		cpkGRAYColor.Enabled = False
	End If
End Sub

Sub Form_OnDestroy()
	Data.Setvalue "\Editor\Back", OldEditorBackColor
	obj=nil
	img=nil
	frame=nil
	s=nil
End Sub

Sub Form_OnOK()
	If edtX.ValueInt = 0 Then edtX.ValueInt = 100
	If edtY.ValueInt = 0 Then edtY.ValueInt = 100
	Data.Setvalue "\CreateImage\X",edtX.ValueInt
	Data.Setvalue "\CreateImage\Y",edtY.ValueInt 
	Data.Setvalue "\CreateImage\GRAY",rdbGRAY.ItemChecked +1
	
	Set obj=Docs.GetActiveDocument( "Image" )
	If TypeName(obj)="Nothing" Then	  
		ActionManager.FileNew  "Image" 
	End If

	Set img = Objects.CreateObject( "Image" )
	If rdbGRAY.ItemChecked = 0 Then
		Data.SetValue "\Editor\Back", cpkGRAYColor.GetColorRGB
		img.CreateNew edtX.ValueInt, edtY.ValueInt, "GRAY"
	Else
		Data.SetValue "\Editor\Back", cpkRGBColor.GetColorRGB
		img.CreateNew edtX.ValueInt, edtY.ValueInt, "RGB"
	End If
	Set frame = MainWnd.ActiveFrame
	Set s = frame.GetSplitter
	Objects.SetToActiveDocument img
	s.SetViewType 0, 0, "ImageDoc.ImageView"
	ActionManager.FitDoc2Screen

	Data.SetValue "\CreateImage\GRAYColor", cpkGRAYColor.GetColorRGB
	Data.SetValue "\CreateImage\RGBColor", cpkRGBColor.GetColorRGB
End Sub

'***************************************************************
'rdbGRAY event handlers.
'***************************************************************
sub rdbGRAY_OnChangeCheck(lItem)
	rdbGRAY.ItemChecked = 0
	rdbRGB.ItemChecked = -1
	cpkRGBColor.Enabled = False
	cpkGRAYColor.Enabled = True
end sub

'***************************************************************
'rdbRGB event handlers.
'***************************************************************
sub rdbRGB_OnChangeCheck(lItem)
	rdbRGB.ItemChecked = 0
	rdbGRAY.ItemChecked = -1
	cpkRGBColor.Enabled = True
	cpkGRAYColor.Enabled = False
end sub


