sub rdbRange_OnChangeCheck(lItem)
	ParamKey = Data.Getvalue("\StatUI\ChartParams\Axis\X_ParamKey")
	ParamCoeff = global.GetParamCoeff(ParamKey)
	If ParamCoeff = 0 Then Exit Sub
	Application.WritePrivateProfileString Data.Getvalue ("\Classes\ClassFile"),"ClassifierInfo","UseRange",rdbRange.ItemChecked
	Application.WritePrivateProfileString Data.Getvalue ("\Classes\ClassFile"),"ClassifierInfo","RangeMaxVal", Application.VtStr(edtMax.ValueDouble / ParamCoeff,"%e")
	Application.WritePrivateProfileString Data.Getvalue ("\Classes\ClassFile"),"ClassifierInfo","RangeMinVal", Application.VtStr(edtMin.ValueDouble / ParamCoeff,"%e")
	Data.SetValue "\StatCfg\OneParam", ParamKey
	global.ObjectSetValue global.GetStatObj(), "\StatCfg\X1", edtMin.ValueDouble
	global.ObjectSetValue global.GetStatObj(), "\StatCfg\X2", edtMax.ValueDouble
	global.ObjectSetValue global.GetStatObj(), "\StatCfg\OneParam", ParamKey
	
	If lItem = 0 Then
		global.ObjectSetValue global.GetStatObj(), "\StatCfg\AxisXAuto", 1
	Else
		Data.Setvalue "\StatCfg\AxisXAuto", 0
		global.ObjectSetValue global.GetStatObj(), "\StatCfg\AxisXAuto", 0
		Data.Setvalue "\StatUI\ChartParams\Axis\X_Begin",Application.VtStr(edtMin.ValueDouble / ParamCoeff,"%e")
		Data.Setvalue "\StatUI\ChartParams\Axis\X_End",	Application.VtStr(edtMax.ValueDouble / ParamCoeff,"%e")
	End If

	EnableRange()
	CalcClassCount()
	FillClasses()
	If NeedRebuild Then	
		Freeze 1
		global.RebuildStat()
		Freeze 0
	End If
end sub
