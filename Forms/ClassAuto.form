sub cbbParam_OnSelectChange()
	Data.Setvalue "\StatUI\ChartParams\Axis\X_ParamKey" , KeyArr(cbbParam.GetSel())
	If Data.Getvalue ("\StatUI\ChartParams\Axis\X_AxisType")=1 Then
		Data.Setvalue "\StatUI\ChartParams\XParamName" , global.GetParamUserName(KeyArr(cbbParam.GetSel()))
	End If
	AllClasses()
	Application.WritePrivateProfileString Data.Getvalue ("\Classes\ClassFile"),"KeysInfo","KeysEnum",KeyArr(cbbParam.GetSel())
	rdbRange_OnChangeCheck(rdbRange.ItemChecked)
end sub

Sub FillParamLst()
	cbbParam.DeleteContents()
	key = Application.GetPrivateProfileString(Data.Getvalue ("\Classes\ClassFile"),"KeysInfo","KeysEnum","")
	If key <> "" Then sParam = global.GetParamUserName(key)
	If not global.isDocImage or global.GetNameStatTable() = "" and global.GetNameObjectList() = "" Then 
		NData.SetSection "\measurement\parameters"
		j = 0
		For i = 0 To NData.GetEntriesCount - 1
			key1 = global.GetParamKey (NData.GetEntryName(i))
			If IsEmpty(NData.GetValue ("\measurement\groups\Extend\"+NData.GetEntryName(i))) Then
				sParam = global.GetParamUserName(key1)
				cbbParam.InsertString j,sParam
				KeyArr(j) = key1
				If key = cstr(key1) Then n = j
				j = j + 1 
			End If
		Next
		cbbParam.SetSel(n)
		Exit Sub
	End If
	If not global.isDocImage or global.GetNameStatTable() = "" and global.GetNameObjectList() = "" Then 
		cbbParam.InsertString 0,sParam
		cbbParam.SetSel 0
		cbbParam.Enabled 0
		Exit Sub
	End If
	n = -1
	If global.GetNameStatTable() <> "" Then
		Set objStatTable = global.GetStatTable()
			pos = objStatTable.getFirstParamPos()
			i = 0
			While pos <> 0
				key = objStatTable.getParamKey(pos)
				If key >=0  Then				
					ParST = global.GetParamUserName(key) 
					If sParam = ParST  Then n = i
					cbbParam.InsertString i,ParST
					KeyArr(i) = key
					i = i + 1 
				End If
				pos = objStatTable.getNextParamPos(pos)			 
			Wend
		Set objStatTable = Nothing 	
	else
		Set ObjList = Objects.GetActiveObject( "Object List" )
		If not Docs.IsPointerNull(ObjList) Then 
			lParamCount = ObjList.GetParametersCount
			lpos = ObjList.GetFirstParameterPosition
			i = 0
			While lpos <> 0	
				lpos = ObjList.GetNextParameter( lpos, strName, strUnits, lkey, fcalibr )
				cbbParam.InsertString i,strName
				If sParam = strName Then n = i
				KeyArr(i) = lkey
				i = i + 1 				
			Wend	
		End If
		Set ObjList = Nothing
	End If
	cbbParam.SetSel(n)
End Sub


