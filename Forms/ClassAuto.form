Sub CalcClassCount()
	If edtStep.ValueDouble > 0 and rdbStepCount.ItemChecked Then
		If rdbRange.ItemChecked = 1 Then
			mn = edtMin.ValueDouble
			mx = edtMax.ValueDouble
		else
			nClass = CInt(Application.GetPrivateProfileString(Data.Getvalue ("\Classes\ClassFile"),"Classes","ClassCount","1"))
			Application.WritePrivateProfileString Data.Getvalue ("\Classes\ClassFile"),"Classes","ClassCount",1
			If global.bNoParam = 0 Then ClassifyObjectList "", global.GetNameStatTable()
			global.CalcMinMax mn,mx
			ParamCoeff = global.GetParamCoeff(Data.Getvalue("\StatUI\ChartParams\Axis\X_ParamKey"))
			If ParamCoeff=0 Then Exit Sub
			mn = mn * ParamCoeff
			mx = mx * ParamCoeff
			Application.WritePrivateProfileString Data.Getvalue ("\Classes\ClassFile"),"Classes","ClassCount",nClass
		End If
		
		n = (mx-mn)/edtStep.ValueDouble
		If n > 30 Then
'			edtCount.ValueInt = 30
			nClass = 30
			edtStep.ValueDouble = FormatNumber((mx-mn)/30+0.0005,3)
			str = Docs.LoadString ("Minimum step:")+" "+cstr(FormatNumber((mx-mn)/30+0.0005,3))
			MsgBox Docs.LoadString ("Maximum number of classes: 30")+vbLf+str, vbOKOnly+vbExclamation,Docs.LoadString ("Attention!")
		else
'			edtCount.ValueInt = Int(n+.999999999)
			nClass = Int(n+.999999999)
		End If
		
		If nClass = 0 Then nClass = 1
		Application.WritePrivateProfileString Data.Getvalue ("\Classes\ClassFile"),"Classes","ClassCount",nClass
		Application.WritePrivateProfileString Data.Getvalue ("\Classes\ClassFile"),"ClassifierInfo","DivisionValue", Application.VtStr(edtStep.ValueDouble	 / ParamCoeff,"%e")

		If global.bNoParam = 0 Then ClassifyObjectList "", global.GetNameStatTable()
		AllClasses()
	End If
End Sub


sub AllClasses()
	swhat = ""
	sOn = ""
 	nClass = CInt(Application.GetPrivateProfileString(Data.Getvalue ("\Classes\ClassFile"),"Classes","ClassCount","1"))
	For i=0 to nClass-1
		If len(sWhat)>0 Then sWhat = sWhat + ","
		sWhat = sWhat + Cstr(i)
		If len(sOn)>0 Then sOn = sOn + ","
		sOn = sOn + Cstr(i)
	Next
	global.ObjectSetValue global.GetStatTable(),"\StatUI\CalcForAllClasses", sWhat 
	global.ObjectSetValue global.GetStatTable(),"\StatUI\CalcForRelativeClasses", sOn
	Data.Setvalue "\StatUI\CalcForAllClasses" ,swhat
	Data.Setvalue "\StatUI\CalcForRelativeClasses" ,sOn
	Application.WritePrivateProfileString Data.Getvalue ("\Classes\ClassFile"),"Select","What",sWhat 
	Application.WritePrivateProfileString Data.Getvalue ("\Classes\ClassFile"),"Select","On",sOn
end sub


