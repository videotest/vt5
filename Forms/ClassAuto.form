sub Form_OnInitialize()
	global.ShowHidePropertyZone 0
	edtStep.ValueDouble = Application.VtDbl(Data.Getvalue ("\StatCfg\Step"))
	Form.ShowButtons 0
	btnOk.ButtonText = Docs.LoadString ("ButtonClose")	
	btnHelp.ButtonText = Docs.LoadString ("Help")
	btnClass.SetPressedState
	btnSetup.ResetPressedState
	btnAxis.ResetPressedState
	btnSelection.ResetPressedState
	btnGraph.ResetPressedState
	Control431.Text = Docs.LoadString ("Automatic Classifier")
	btnSetup.ToolTip = Docs.LoadString ("Titles")
	btnAxis.ToolTip = Docs.LoadString ("Axes")
	btnSelection.ToolTip = Docs.LoadString ("Sample")
	btnGraph.ToolTip = Docs.LoadString ("Statistics")
	btnClass.ToolTip = Docs.LoadString ("Classifier")
	btnNew.ButtonText = Docs.LoadString ("New")
	btnCopy.ButtonText = Docs.LoadString ("Copy")
	btnDelete.ButtonText = Docs.LoadString ("Delete")
	btnNew.ToolTip = Docs.LoadString ("New Classifier")
	btnCopy.ToolTip = Docs.LoadString ("Copy Classifier")
	btnDelete.ToolTip = Docs.LoadString ("DeleteClassifier")
	txtParam.Text = Docs.LoadString ("Parameter")
	txtTitle.Text = Docs.LoadString ("Classification Settings")
	btnCalcStep.ToolTip = Docs.LoadString ("Recalculate")
	sDelete = Docs.LoadString ("DeleteClassifier")

	rdbStepCount.DeleteAll
	rdbStepCount.AddItem Docs.LoadString ("Number")
	rdbStepCount.AddItem Docs.LoadString ("Step")
	rdbStepCount.ReCreate
	
	rdbRange.DeleteAll
	rdbRange.AddItem Docs.LoadString ("Full range")
	rdbRange.AddItem Docs.LoadString ("From...to")
	rdbRange.ReCreate

	edtMin.ValueDouble = Application.VtDbl(Data.Getvalue ("\StatCfg\X1"))
	edtMax.ValueDouble = Application.VtDbl(Data.Getvalue ("\StatCfg\X2"))

	Data.Setvalue "\StatCfg\RunClass" , ""
	Data.Setvalue "\StatCfg\RunForm", ""
	global.ObjectSetValue global.GetStatObj(), "\StatCfg\RunForm", ""

	iUnknown = 0
	lstClassifier.SetColumnCaption 0, Docs.LoadString ("Classifier")
	lstClassifier.SetColumnCaption 1, Docs.LoadString ("Type")
	global.sClassifier = Data.Getvalue ("\Classes\ClassFile")
	If Docs.IsPointerNull(Data) Then Exit Sub
	Set ObjList = Objects.GetActiveObject( "Object List" )
	Set NData = Data
	If global.GetNameStatTable() <> "" Then
		Set NData = global.GetTableNdata() 
	ElseIf not Docs.IsPointerNull(ObjList) Then 
		Set NData = ObjList.GetPrivateNamedData
	End If
	Set ObjList = Nothing	
	
	ClsType = global.ClassifierType (Data.Getvalue ("\Classes\ClassFile"))
	If ClsType = "A" Then
		FillListClassifier()
		LoadClassifier()
	End If	
	If Data.Getvalue ("\StatUI\ChartParams\Axis\X_AxisType") = 0 Then
		chkLog.Disabled = 1
	Else
		chkLog.Disabled = 0
	End If
	FillParamLst()	
	TestReadOnly()
	MainWnd.SetWindowLock False,True
	If Data.Getvalue ("\General\tmp") = "GlobalStat" Then EndClass()			
end sub
