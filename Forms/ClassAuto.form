sub chkLog_OnCheck(nCheck)
	Data.Setvalue "\StatUI\ChartParams\UniformDistribution", chkLog.Checked
	Data.Setvalue "\StatUI\ChartParams\Axis\X_UseLogaScale", chkLog.Checked
	Application.WritePrivateProfileString Data.Getvalue ("\Classes\ClassFile"),"Classes","ClassCount", edtCount.ValueInt
	If nCheck Then
		Application.WritePrivateProfileString Data.Getvalue ("\Classes\ClassFile"),"ClassifierInfo","DivisionType",2
	else
		Application.WritePrivateProfileString Data.Getvalue ("\Classes\ClassFile"),"ClassifierInfo","DivisionType",0
	End If
	Freeze 1
	EnableStep()
	global.ReplaceLogParams()
	global.RebuildStat()
	FillClasses()
	Freeze 0
end sub