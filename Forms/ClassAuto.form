sub btnDelete_Click()
		str = lstClassifier.GetCellAsString (0,lstClassifier.GetCurentRow )
	Answer = MsgBox(sDelete & " """ & str & """?", vbQuestion + vbYesNo, global.sVT)
	If Answer = vbYes Then
		str = Data.Getvalue ("\Paths\Classes") + str + strClassExt
		If fso.FileExists(str) Then fso.DeleteFile str
		FillListClassifier()
		lstClassifier_CurentRowChange lstClassifier.GetCurentRow()
	End If
	If lstClassifier.GetRowCount() = 0 Then
		btnNew_Click()	
		FillListClassifier()
		If lstClassifier.GetRowCount() = 0 Then
			Str = Data.Getvalue ("\Paths\Classes") + global.sDefaultClass
			global.FileCopy Data.Getvalue ("\Paths\Classes")+"a.cls", Str 
			FillListClassifier()
		End If
		lstClassifier_CurentRowChange 0
	End If
end sub

sub SaveClassTable()
	clr = Data.Getvalue ("\Classes\UnkClassColor")
	Str = clr + ",?"
	Application.WritePrivateProfileString Data.Getvalue ("\Classes\ClassFile"),"Classes",-1,Str
	For r=0 To lstLimits.GetRowCount()-1
		str1 = lstLimits.GetCellAsString(0,r)
		str2 = lstLimits.GetCellAsColorRGB(1,r)
		Str = str2 + "," + str1
		Application.WritePrivateProfileString Data.Getvalue ("\Classes\ClassFile"),"Classes",CStr(r),Str
	Next
	Application.WritePrivateProfileString Data.Getvalue ("\Classes\ClassFile"),"Classes","ClassCount",lstLimits.GetRowCount()
	global.AClassifierToDoc()
end sub

