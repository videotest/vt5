Sub FillClasses()

	For i=lstLimits.GetRowCount() to  0 step -1
		lstLimits.DeleteRow i
	Next
	lstLimits.SetColumnCaption 0, Docs.LoadString("Class")
	lstLimits.SetColumnCaption 1, Docs.LoadString("Color")
	nClass = CInt(Application.GetPrivateProfileString(Data.Getvalue ("\Classes\ClassFile"),"Classes","ClassCount","1"))
	For i=0 to nClass-1
		lstLimits.AddRow(-1)
		StrFromINI = Application.GetPrivateProfileString(Data.Getvalue ("\Classes\ClassFile"),"Classes",CStr(i),"-")
		StrFromINI = Mid(StrFromINI, InStr(StrFromINI,")," )+2,255)
		If StrFromINI = "" Then StrFromINI = Cstr(i+1)
		lstLimits.SetCellValue 0,i,StrFromINI
		StrFromINI = Application.GetPrivateProfileString(Data.Getvalue ("\Classes\ClassFile"),"Classes",CStr(i),"0")
		StrFromINI = Left(StrFromINI, InStr(StrFromINI,")," ))
		If StrFromINI = "" Then StrFromINI = "(0,128,128)"
		lstLimits.SetCellValueColorRGB 1,i,StrFromINI
	Next
End Sub

