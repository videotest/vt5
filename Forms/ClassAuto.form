sub TestReadOnly()
	If fso.FileExists(Data.Getvalue ("\Classes\ClassFile")) Then
			Set objFile = fso.GetFile(Data.Getvalue ("\Classes\ClassFile"))
			If objFile.Attributes And 5 Then
				DisableControls 1
				DisableAll 1
				lstClassifier.SetColumnReadOnly 0,1
				btnCopy.Disabled = 0
			else
				DisableControls 0
				DisableAll 0
				EnableStep()
				EnableButtonsStat()
				EnableRange()
				lstClassifier.SetColumnReadOnly 0,0
			End If
			Set objFile = Nothing
	End If	
end sub

Sub DisableAll(d)
	cbbParam.Enabled = d-1
	edtCount.Disabled = d
	rdbStepCount.Enabled = d-1
End Sub

Sub EndClass()
	global.sClassifier = Data.Getvalue ("\Classes\ClassFile")
	If Not Docs.IsPointerNull(MethodMan) Then 
		MethodMan.StartSetValueLogging
		Data.Setvalue "\Classes\ClassFile" , global.sClassifier	
		MethodMan.FinishSetValueLogging
	End If
	FormManager.PressOk
End Sub
