sub lstClassifier_DataChange(nColumn, nRow)
	If nColumn=0 and IfNameExist(lstClassifier.GetCellAsString (0,nRow)) Then
		MsgBox Docs.LoadString ("This name already exist!\nPlease, change it."), vbOKOnly+vbExclamation,Docs.LoadString ("Attention!")
		lstClassifier.ForceEditCell 0,lstClassifier.GetCurentRow
	else
		src = Data.Getvalue ("\Paths\Classes") + NameCopy(nRow) + strClassExt
		tag = Data.Getvalue ("\Paths\Classes") + lstClassifier.GetCellAsString (0,nRow) + strClassExt
		global.FileCopy src,tag
		If fso.FileExists(src) and fso.FileExists(tag) Then
			fso.DeleteFile src
			Data.Setvalue "\Classes\ClassFile" , tag
			global.sClassifier = Data.Getvalue ("\Classes\ClassFile")
		End If
	End If
end sub

sub lstClassifier_CurentRowChange(nRow)
	If rec Then Exit Sub
	global.iChangeClass = 1
	If iUnknown = 1 Then
		lstClassifier.DeleteRow lstClassifier.GetRowCount()-1
		iUnknown = 0
		DisableControls 0
	End If
	cls = lstClassifier.GetCellAsString (0,lstClassifier.GetCurentRow )
	If cls <> "" Then
		Data.Setvalue "\Classes\ClassFile" , Data.Getvalue ("\Paths\Classes") + cls + strClassExt
		global.sClassifier = Data.Getvalue ("\Classes\ClassFile")
	End If	
	Select Case global.ClassifierType(Data.Getvalue ("\Classes\ClassFile"))
		Case "M"
			Data.Setvalue "\StatCfg\RunForm", "class"
			Data.Setvalue "\StatCfg\RunClass" , "M"
			EndClass()			
		Case "F"
			Data.Setvalue "\StatCfg\RunForm", "class"
			Data.Setvalue "\StatCfg\RunClass" , "F"
			EndClass()			
		Case "A"
			LoadClassifier()
'			global.RebuildStat()
		Case else
	End Select
	TestReadOnly()
	global.iChangeClass = 0
end sub

Sub FillListClassifier()
	Dim f, flist, objf
	Dim strName
	ClearList()
	rec = 1
'	If iUnknown = 1 Then
'		lstClassifier.DeleteRow lstClassifier.GetRowCount()-1
'		iUnknown = 0
'		DisableControls 0
'	End If
	If fso.FolderExists(Data.Getvalue ("\Paths\Classes")) Then
		Set f = fso.GetFolder(Data.Getvalue ("\Paths\Classes"))
		Set flist = f.Files
		i = 0
		For Each objf In flist
			If instr(objf.Name,".class") Then
				strName = global.GetFileName(objf.Name)
				If strName <> ""  Then
					lstClassifier.AddRow(-1)
					lstClassifier.SetCellValue 0,i, strName
					str = global.ClassifierType(Data.Getvalue ("\Paths\Classes")+objf.Name )
					str = Docs.LoadString(str)
					lstClassifier.SetCellValue 1,i, str
					i=i+1
				End If
			End If
		Next
	End If
	ReDim NameCopy (lstClassifier.GetRowCount())
	ok = 0
	For i=0 to  lstClassifier.GetRowCount()-1
		NameCopy(i) = lstClassifier.GetCellAsString(0,i)
		If global.GetFileName(Data.Getvalue ("\Classes\ClassFile")) = lstClassifier.GetCellAsString (0,i) Then
			lstClassifier.SetCurentRow i
			ok = 1
		End If
	Next
	If ok=0 Then lstClassifier.SetCurentRow 0
	rec = 0
End Sub

Sub ClearList()
	rec = 1
	For i=lstClassifier.GetRowCount() to  0 step -1
		lstClassifier.DeleteRow i
	Next
	rec = 0
End Sub

Sub LoadClassifier()
	Param = Application.GetPrivateProfileString(Data.Getvalue ("\Classes\ClassFile"),"KeysInfo","KeysEnum","")
	If Param = "" or instr(Param,",") Then
		bPar = false
		rdbStepCount.ItemChecked = 0
		DisableControls 1
		lstLimits.Disabled 0
		edtCount.Disabled 0
'		MsgBox "Ошибка в параметрах классификатора"
'		exit sub
	else	
		bPar = true
		DisableControls 0
	End If
	NeedRebuild = 0
	If global.StatDiffParam() Then 
		NeedRebuild = 1
	Data.Setvalue "\StatUI\ChartParams\Axis\X_ParamKey" , Param
	End If
	If bPar Then 
		ParamKey = CLng(Param)
		Param = global.GetParamUserName(CLng(Param))
		If Data.Getvalue ("\StatUI\ChartParams\Axis\X_AxisType")=1 Then
			Data.Setvalue "\StatUI\ChartParams\XParamName" , Param
		End If
	'	Data.Setvalue "\StatUI\StatTable\Title" , Param
	End If
	DivisionType = Application.GetPrivateProfileString(Data.Getvalue ("\Classes\ClassFile"),"ClassifierInfo","DivisionType",0)
	If DivisionType=2 Then
		chkLog.Checked = 1	
	Else
		chkLog.Checked = 0
	End If
	edtCount.Text = Application.GetPrivateProfileString(Data.Getvalue ("\Classes\ClassFile"),"Classes","ClassCount",3)
	If not bPar Then Exit Sub
	
	DivisionType = Application.GetPrivateProfileString(Data.Getvalue ("\Classes\ClassFile"),"ClassifierInfo","DivisionType",0)
	Select Case DivisionType
		Case 0
			If Data.Getvalue ("\StatUI\ChartParams\UniformDistribution")=1 Then 
			Data.Setvalue "\StatUI\ChartParams\UniformDistribution", 0
				global.ReplaceLogParams()
			End If
			Data.Setvalue "\StatUI\ChartParams\Axis\X_UseLogaScale", 0
			rdbStepCount.ItemChecked = 0
		Case 1
			If Data.Getvalue ("\StatUI\ChartParams\UniformDistribution")=1 Then 
			Data.Setvalue "\StatUI\ChartParams\UniformDistribution", 0
				global.ReplaceLogParams()
			End If
			Data.Setvalue "\StatUI\ChartParams\Axis\X_UseLogaScale", 0
			rdbStepCount.ItemChecked = 1
			CalcClassCount()	
		Case 2
			rdbStepCount.ItemChecked = 0
			If Data.Getvalue ("\StatUI\ChartParams\UniformDistribution")=0 Then 
			Data.Setvalue "\StatUI\ChartParams\UniformDistribution", 1
				global.ReplaceLogParams()
			End If
			Data.Setvalue "\StatUI\ChartParams\Axis\X_UseLogaScale", 1
	End Select
	
	rdbRange.ItemChecked = Application.GetPrivateProfileString(Data.Getvalue ("\Classes\ClassFile"),"ClassifierInfo","UseRange",0)
	rdbRange_OnChangeCheck rdbRange.ItemChecked
	global.ObjectSetValue global.GetStatObj(), "\StatCfg\AxisXAuto", abs(rdbRange.ItemChecked - 1)
	If not Docs.IsPointerNull(global.GetStatNdata()) Then		
		Set objStatData = global.GetStatNdata()
		If not Docs.IsPointerNull(objStatData) Then		
			If not global.ExistAutoClassifier(Data.Getvalue ("\Classes\ClassFile"),objStatData) Then
				NeedRebuild = 1
			End If
			Set objStatData = Nothing
		End If
	End If	
	FillClasses()
	SaveClassTable()

	Data.SetValue "\StatUI\ChartParams\UseCustomIntervals", 1
	Data.SetValue "\StatCfg\OneParam", ParamKey
	If Data.Getvalue("\StatUI\ChartParams\Axis\X_ParamKey") <> Application.GetPrivateProfileString(Data.Getvalue ("\Classes\ClassFile"),"KeysInfo","KeysEnum","") Then
		Data.Setvalue "\StatUI\ChartParams\Axis\X_AxisType", 0
	End If
	EnableRange()
	EnableStep()
	global.ReadClassSelection()
	If NeedRebuild Then 
		global.RebuildStat()
	else
		If global.bNoParam = 0 Then ClassifyObjectList 
	End If
	NeedRebuild = 1
	FillParamLst()
	Freeze 0
End Sub

