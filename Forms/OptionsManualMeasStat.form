[Script]
Dim global
Set global = Application.GetglobalScript("")

Form.Title = Docs.LoadString ("Statistics")

Sub Form_OnOK()	
	Data.Setvalue "\measurement\ShowResult61460", Abs(chkCount.Checked)
	Data.Setvalue "\measurement\ShowResult61463", Abs(chkSum.Checked)
	Data.Setvalue "\measurement\ShowResult61450", Abs(chkAverage.Checked)
	Data.Setvalue "\measurement\ShowResult61451", Abs(chkCKO.Checked)
	Data.Setvalue "\measurement\ShowResult61453", Abs(chkAbsErr.Checked)
	Data.Setvalue "\measurement\ShowResult61452", Abs(chkCVar.Checked)
	Data.Setvalue "\measurement\ShowResult61454", Abs(chkMin.Checked)
	Data.Setvalue "\measurement\ShowResult61455", Abs(chkMax.Checked)
	Form.UpdateSystemSettings
End Sub

Sub Form_OnInitialize()
	chkCount.Caption = Docs.LoadString ("Count")
	chkSum.Caption = Docs.LoadString ("Sum")
	chkCKO.Caption = Docs.LoadString ("Std. Deviation")
	chkCVar.Caption = Docs.LoadString ("Var. Coeff.")
	chkMin.Caption = Docs.LoadString ("Minimum")
	chkMax.Caption = Docs.LoadString ("Maximum")
	If global.isMaster Then
		chkAverage.Caption = Docs.LoadString ("Arithm. Mean")
		chkAbsErr.Caption = Docs.LoadString ("Confidence Interval")
	Else
		chkAverage.Caption = Docs.LoadString ("Average")
		chkAbsErr.Caption = Docs.LoadString ("Abs. Error")
	End If

	If Data.Getvalue ("\measurement\ShowResult61460") Then chkCount.Checked 1
	If Data.Getvalue ("\measurement\ShowResult61463") Then chkSum.Checked 1
	If Data.Getvalue ("\measurement\ShowResult61450") Then chkAverage.Checked 1
	If Data.Getvalue ("\measurement\ShowResult61451") Then chkCKO.Checked 1
	If Data.Getvalue ("\measurement\ShowResult61453") Then chkAbsErr.Checked 1
	If Data.Getvalue ("\measurement\ShowResult61452") Then chkCVar.Checked 1
	If Data.Getvalue ("\measurement\ShowResult61454") Then chkMin.Checked 1
	If Data.Getvalue ("\measurement\ShowResult61455") Then chkMax.Checked 1	
End Sub