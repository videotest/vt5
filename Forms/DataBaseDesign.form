Dim sDate, sNumeric, sDBText, sObject, sRect, sLine, sText, sImage

'***************************************************************
'Form event handlers.
'***************************************************************
Sub Form_OnInitialize()

	sDate = Docs.LoadString ("Database Date Field")
	sNumeric = Docs.LoadString ("Database Numeric Field")
	sDBText = Docs.LoadString ("Database Text Field")

	Select Case Data.GetValue ("\General\ProgramName")
	  Case "Karyo"
		sObject = Docs.LoadString ("Image/Karyogram")
	  Case "Metal"
		sObject = Docs.LoadString ("Image/Statistics")
	  Case Else
		sObject = Docs.LoadString ("Image/Measurements/Statistics")
	End Select

	sRect = Docs.LoadString ("3D Rectangle Frame with Text")
	sLine = Docs.LoadString ("Straight Line")
	sText = Docs.LoadString ("Text (Form Design)")
	sImage = Docs.LoadString ("Image")
	
	txtInsertCaption.Text = Docs.LoadString("Database Fields")
	Select Case Data.GetValue ("\General\ProgramName")
	  Case "Master"
		MainWnd.XPSetDefaultStatusText Docs.LoadString("Form design and database editing")
	  Case Else
		MainWnd.XPSetDefaultStatusText Docs.LoadString("Database Form Design mode.\n\nYou can move database fields, insert new ones (use the buttons in the dialog box), delete the unnecessary ones (use the Delete key or the command in the field context menu).")
	End Select

	btnDateTime_ctrl.ToolTip = Docs.LoadString ("Database Date Field")
	btnNumber_ctrl.ToolTip = Docs.LoadString ("Database Numeric Field")
	
	Select Case Data.GetValue ("\General\ProgramName")
	  Case "Karyo"
		btnObject_ctrl.ToolTip = Docs.LoadString ("Image/Karyogram")
	  Case "Metal"
		btnObject_ctrl.ToolTip = Docs.LoadString ("Image/Statistics")
	  Case "Master"
		btnObject_ctrl.ToolTip = Docs.LoadString ("Image/Measurements")
		btnStatDiag.ToolTip = Docs.LoadString ("Statistics diagram")
		btnStatTable.ToolTip = Docs.LoadString ("Statistics table")
		btnStatLegend.ToolTip = Docs.LoadString ("Statistics legend")
	  Case Else
		btnObject_ctrl.ToolTip = Docs.LoadString ("Image/Measurement Table")
	End Select
	
	btnDBText_ctrl.ToolTip = Docs.LoadString ("Database Text Field")
	btnRectangl_ctrl.ToolTip = Docs.LoadString ("3D Rectangle Frame with Text")
	btnLine_ctrl.ToolTip = Docs.LoadString ("Straight Line")
	btnText_ctrl.ToolTip = Docs.LoadString ("Text (Form Design)")
	btnImage_ctrl.ToolTip = Docs.LoadString ("Image")

	btnUndo.ToolTip = Docs.LoadString ("Undo Operation")
	btnRedo.ToolTip = Docs.LoadString ("Redo Operation")
	
	btnUndo.Disabled = True
	btnRedo.Disabled = True

	ActionState.ClearUndoList
	
	strDBToolbarName = LCase(Docs.LoadString("Database"))
	For i = 0 To ToolBars.GetBarsCount - 1
'		MsgBox "." & strDBToolbarName & ". = ." & LCase(ToolBars.GetBarName(i)) & "."
		If LCase(Trim(ToolBars.GetBarName(i))) = strDBToolbarName Then
'			MsgBox i
			ToolBars.ShowBar i, 0
		End If
	Next
End Sub

Sub Form_OnDestroy()
	MainWnd.XPSetDefaultStatusText " "

	strDBToolbarName = LCase(Docs.LoadString("Database"))
	For i = 0 To ToolBars.GetBarsCount - 1
		If LCase(Trim(ToolBars.GetBarName(i))) = strDBToolbarName Then
			ToolBars.ShowBar i, 1
		End If
	Next
End Sub

Sub Form_OnOK()
End Sub

Sub Form_OnCancel()
	ActionState.TerminateCurrentAction
	If a=0 Then
		FormManager.PressCancel	
		a=1
	End If
End Sub


