Dim strActiveControl

Function InsertAxControl(strType_ctrl)
	'Прорисовка кнопки Undo на форме //
	Dim Ctrl
	Ctrl=1
	ActionState.TerminateCurrentAction
	Null_Action
	
	If strType_ctrl = strActiveControl Then
		ActionState.TerminateCurrentAction
		strActiveControl = ""
		Exit Function
	End If 
	strActiveControl = strType_ctrl
	btnDateTime_ctrl.ResetPressedState()
	btnNumber_ctrl.ResetPressedState()
	btnObject_ctrl.ResetPressedState()	
	btnDBText_ctrl.ResetPressedState()
	btnRectangl_ctrl.ResetPressedState()
	btnLine_ctrl.ResetPressedState()
	btnImage_ctrl.ResetPressedState()
	btnText_ctrl.ResetPressedState()
	btnObject_ctrl.ResetPressedState()
	btnStatDiag.ResetPressedState()
	btnStatLegend.ResetPressedState()
	btnStatTable.ResetPressedState()
	
	msgBox 2

	select case strType_ctrl
		case "DateTime"
			btnDateTime_ctrl.SetPressedState()
			txtControlName.Text = sDate
			AxInsertControl "DBCONTROLS.DBDateTimeCtrl.1"
			btnDateTime_ctrl.ResetPressedState()
		case "Number"
			btnNumber_ctrl.SetPressedState()
			txtControlName.Text = sNumeric
			Call AxInsertControl ("DBCONTROLS.DBNumberCtrl.1")
			btnNumber_ctrl.ResetPressedState()
		case "Statistisc"
			btnObject_ctrl.SetPressedState()
			txtControlName.Text = sObject
			Call AxInsertControl ("DBCONTROLS.DBObjectCtrl.1")
			btnObject_ctrl.ResetPressedState()
		case "Object"
			btnObject_ctrl.SetPressedState()
			txtControlName.Text = sObject
			Call AxInsertControl ("DBCONTROLS.DBObjectCtrl.1")
			btnObject_ctrl.ResetPressedState()
		case "StatDiag"
			btnStatDiag.SetPressedState()
			txtControlName.Text = sObject
			AxInsertControl "DBCONTROLS.DBObjectCtrl.1", CLng(1)
			btnStatDiag.ResetPressedState()
		case "StatLegend"
			btnStatLegend.SetPressedState()
			txtControlName.Text = sObject
			AxInsertControl "DBCONTROLS.DBObjectCtrl.1", CLng(2)
			btnStatLegend.ResetPressedState()
		case "StatTable"
			btnStatTable.SetPressedState()
			txtControlName.Text = sObject
			Call AxInsertControl ("DBCONTROLS.DBObjectCtrl.1", CLng(4))
			btnStatTable.ResetPressedState()
		case "DBText"
			btnDBText_ctrl.SetPressedState()
			txtControlName.Text = sDBText
			Call AxInsertControl ("DBCONTROLS.DBTextCtrl.1")
			btnDBText_ctrl.ResetPressedState()
		case "Rectangle"
			btnRectangl_ctrl.SetPressedState()
			txtControlName.Text = sRect
			Call AxInsertControl ("VTDESIGN.VTD3DBarCtrl.1")
			btnRectangl_ctrl.ResetPressedState()
		case "Line"
			btnLine_ctrl.SetPressedState()
			txtControlName.Text = sLine
			Call AxInsertControl ("VTDESIGN.VTDLineCtrl.1")
			btnLine_ctrl.ResetPressedState()
		case "Text"
			btnText_ctrl.SetPressedState()
			txtControlName.Text = sText
			Call AxInsertControl ("VTDESIGN.VTDTextCtrl.1")
			btnText_ctrl.ResetPressedState()
		case "Image"
			btnImage_ctrl.SetPressedState()
			txtControlName.Text = sImage
			Call AxInsertControl ("VTDESIGN.VTDImageCtrl.1")
			btnImage_ctrl.ResetPressedState()
	End select
	txtControlName.Text = " "
	strActiveControl = ""
	Null_Action		'Прорисовка кнопки Undo при нажатии Cancel когда активна кнопка AX-контрола //
	InsertAxControl=Ctrl
End Function

