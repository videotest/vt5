[Script]

Dim fso
Dim global 
Set global = Application.GetglobalScript("")
Set fso = CreateObject("Scripting.FileSystemObject")

'***************************************************************
'Form event handlers.
'***************************************************************
Sub Form_OnDestroy()
	Set global = Nothing
	Set fso = Nothing
End Sub
Sub Form_OnInitialize()
	Form.EnableButtons 26
	rdbType.DeleteAll
	rdbType.AddItem Docs.LoadString ("Auto")
	rdbType.AddItem Docs.LoadString ("Manual")
	rdbType.AddItem Docs.LoadString ("Without parameters")
	rdbType.ReCreate
	rdbType.ItemChecked 0
	edtName.Text = ""
	edtName.CaptionText Docs.LoadString ("Name:")
	txtSetType.Text Docs.LoadString ("Set Classifier Type")
	txtMsg.Text ""	
End Sub

Sub Form_OnShow()
	edtName.SetFocus() 
End Sub

Sub Form_OnOK()
	Data.Setvalue "\StatCfg\tmp", ""
	If edtName.Text <> "" Then		
		Str = Data.Getvalue ("\Paths\Classes") + Trim(edtName.Text) + ".class"
		Data.Setvalue "\StatCfg\tmp", Str
		Select Case rdbType.ItemChecked 
			Case 0
				global.FileCopy Data.Getvalue ("\Paths\Classes")+"a.cls", Str 
			Case 1
				global.FileCopy Data.Getvalue ("\Paths\Classes")+"m.cls", Str 
			Case 2
				global.FileCopy Data.Getvalue ("\Paths\Classes")+"wp.cls", Str 
		End Select
	End If
End Sub



sub rdbType_OnChangeCheck(lItem)
	edtName.SetFocus() 
End Sub

'***************************************************************
'edtName event handlers.
'***************************************************************
Sub edtName_DataChange()
	Str = Data.Getvalue ("\Paths\Classes") + Trim(edtName.Text) + ".class"
	If Not fso.FileExists(Str) Then
		txtMsg.Text ""
		If Trim(edtName.Text) = "" Then
			Form.EnableButtons 26
		Else
		Form.EnableButtons 27
		End If
	Else
		txtMsg.Text Docs.LoadString ("This classifier already exists!")
		Form.EnableButtons 26
	End If
End Sub