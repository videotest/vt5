Set global = Application.GetGlobalScript("")

Sub SetSelectByName (Name,bSelect)
	Set obj = Objects.GetObjectByName(Name)
	If not Docs.IsPointerNull(obj) Then Objects.SelectObject obj, bSelect
	Set obj = Nothing
End Sub

Form.ShowButtons 0

'***************************************************************
'lstTables event handlers.
'***************************************************************
sub Form_OnInitialize()
	HideDockBars 
	Data.SetValue "\DockBars\NoShowHide",1
	btnOk.ButtonText = Docs.LoadString ("OK")	
	btnHelp.ButtonText = Docs.LoadString ("Help")

	Set obj = Objects.GetActiveObject( "StatObject" )
	If Docs.IsPointerNull(obj) Then
		pos = Objects.GetFirstObjectPos("StatObject")
		If pos <> 0 Then
			Set obj = Objects.GetObject("StatObject", pos)
		End If
	End If
	If not Docs.IsPointerNull(obj) Then 
		Set Ndata = obj.GetPrivateNamedData
		edtMO.ValueDouble = Ndata.Getvalue ("\StatUI\ChartParams\UserCurveMX")
		edtDisp.ValueDouble = Ndata.Getvalue ("\StatUI\ChartParams\UserCurveDX")
		chkCompare.Checked = Ndata.Getvalue ("\StatUI\ChartParams\ShowUserCurve")
		clrColor.SetColorRGB Ndata.Getvalue ("\StatUI\ChartParams\UserCurveColor")
		set Ndata = Nothing
	End If
	set obj = Nothing

	
	iFirst = 1
	CheckData()
	FillLst() 
	CheckCompare()
end sub
sub lstTables_DataChange(nColumn, nRow)
	Select Case nColumn
		Case 0
			sName = lstTables.GetCellAsString (1, nRow)	
			b = lstTables.GetCellAsLogical (0, nRow)
			global.gRec = 1
			SetSelectByName sName, b
			CheckCompare()
			global.gRec = 0
		Case 2
			sColor = lstTables.GetCellAsColorRGB (2,nRow)
			set obj = Objects.GetobjectByName (lstTables.GetCellAsString (1, nRow))
			Set Ndata = obj.GetPrivateNamedData
			If strType = "Distr" Then
				Ndata.Setvalue "\StatUI\ChartParams\ChartColor", sColor
				Ndata.Setvalue "\StatUI\Legend\SamplingColor", sColor
			else
				Ndata.Setvalue "\StatDependence\MarkerColor", sColor
			End If
			set Ndata = Nothing
			set obj = Nothing
			MainWnd.UpdateViewSettings
		Case 3
			sColor = lstTables.GetCellAsColorRGB (3,nRow)
			set obj = Objects.GetobjectByName (lstTables.GetCellAsString (1, nRow))
			Set Ndata = obj.GetPrivateNamedData
			If strType = "Distr" Then
				Ndata.Setvalue "\StatUI\ChartParams\CurveColor", sColor			
			else
				Ndata.Setvalue "\StatDependence\ChartParams\ChartColor", sColor
			End If
			set Ndata = Nothing
			set obj = Nothing
			MainWnd.UpdateViewSettings
		Case Else
	End Select
end sub
'***************************************************************
'Form event handlers.
'***************************************************************
sub Form_OnShow()
end sub
sub Form_OnDestroy()
	
	Set global = Nothing
	Data.SetValue "\DockBars\NoShowHide",0
	ShowDockBars
end sub


Sub CheckCompare()
	n = 0
	For i=0 to lstTables.GetRowCount()-1
		b = lstTables.GetCellAsLogical (0, i)
		If b Then n = n + 1
	Next
	If n = 0 Then Exit Sub
	If strType = "Distr" Then 
		Set_Distr_Scale()
	else
		RecalcAxisDepend()
	End If
	
	If n>1 and strType = "Distr" Then
		Objects.GetFirstSelectedObjectPos "StatObject", lPos
		Set obj = Objects.GetNextSelectedObject("StatObject",lPos)
		Set Ndata = obj.GetPrivateNamedData
		NData.Setvalue "\StatUI\ChartParams\ChartViewType" , 1
		set Ndata = Nothing
		Set obj = Nothing
	End If
	If n=1 Then
		Objects.GetFirstSelectedObjectPos "StatObject", lPos
		If lPos <> 0 Then
			Set obj = Objects.GetNextSelectedObject("StatObject",lPos)
			Set Ndata = obj.GetPrivateNamedData
			NData.Setvalue "\StatUI\ChartParams\ChartViewType" , 0
			set Ndata = Nothing
			Set obj = Nothing
		End If
	End If
	MainWnd.UpdateViewSettings
End Sub

Sub FillLst()
	
	If strType = "Distr" Then
		sType = "StatObject"
	else
		sType = "StatTable"
	End If
		
	For i=lstTables.GetRowCount() to 0 step -1
		lstTables.DeleteRow i
	Next
	lPos = Objects.GetFirstObjectPos(sType)
	For i=0 to Objects.GetObjectCount(sType)-1
		Set obj = Objects.GetObject(sType,lPos)
		lPos = Objects.GetNextObjectPos(sType, lPos)	
		lstTables.AddRow(-1)
		lstTables.SetCellValue 1, i, obj.Name
		Set Ndata = obj.GetPrivateNamedData
		Ndata.Setvalue "\StatUI\Legend\SamplingName", obj.Name
		If strType = "Distr" Then
			sColor = Ndata.Getvalue ("\StatUI\ChartParams\ChartColor")
			sColor2 = Ndata.Getvalue ("\StatUI\ChartParams\CurveColor")
		else
			sColor = Ndata.Getvalue ("\StatDependence\MarkerColor")
			sColor2 = Ndata.Getvalue ("\StatDependence\ChartParams\ChartColor")
		End If
		lstTables.SetCellValueColorRGB 2, i, sColor
		lstTables.SetCellValueColorRGB 3, i, sColor2
		set Ndata = Nothing
		Set obj = Nothing
	Next
	lPos = 0
	Objects.GetFirstSelectedObjectPos sType, lPos
	While lPos <> 0
		Set obj = Objects.GetNextSelectedObject(sType,lPos)
		sName = obj.Name
		Set obj = Nothing
		For i=0 to Objects.GetObjectCount(sType)-1
			If sName = lstTables.GetCellAsString (1, i) Then
				lstTables.SetCellAsLogical 0, i, 1 
			End If			
		Next
	Wend
End Sub

Function CheckData()
	CheckData = 0
	Select Case global.GetCurrentView()
		Case "StatUI.StatObjectView"
			strType = "Distr"
		Case "StatData.StatTableDependenceView"
			RecalcAxisDepend()
			strType = "Depend"
		Case Else
			ShowStatView
			strType = "Distr"
	End Select
End Function

Sub RecalcAxisDepend()
	Dim FirstPos
	x1 = 1e+300
	x2 = 0
	y1 = 1e+300
	y2 = 0
	Objects.GetFirstSelectedObjectPos "StatTable", lPos
	FirstPos = lPos
	While lPos <> 0
		Set obj = Objects.GetNextSelectedObject("StatTable",lPos)
		Set Ndata = obj.GetPrivateNamedData
		
		If iFirst Then
			iFirst = 0
			iFirstX = Ndata.Getvalue ("\StatDependence\ChartParams\Axis\X_ParamKey")
			iFirstY = Ndata.Getvalue ("\StatDependence\ChartParams\Axis\Y_ParamKey")
		else
			Ndata.Setvalue "\StatDependence\ChartParams\Axis\X_ParamKey", iFirstX
			Ndata.Setvalue "\StatDependence\ChartParams\Axis\Y_ParamKey", iFirstY
		End If
		pos = obj.getFirstRowPos()
		While pos <> 0
			v = obj.getValueByKey ( pos, iFirstX )
			x1 = global.Min(v,x1)
			x2 = global.Max(v,x2)
			v = obj.getValueByKey ( pos, iFirstY )
			y1 = global.Min(v,y1)
			y2 = global.Max(v,y2)
			pos = obj.getNextRowPos(pos)
		Wend
		
		Ndata.Setvalue "\StatUI\ChartParams\ChartColor", sColor
		Ndata.Setvalue "\StatUI\Legend\SamplingColor", sColor
		Ndata.Setvalue "\StatUI\ChartParams\CurveColor", sColor			
		set Ndata = Nothing
		Set obj = Nothing
	Wend
	CalcStep()
	Objects.GetFirstSelectedObjectPos "StatTable", lPos
	If lPos <>0 Then	
		Set obj = Objects.GetNextSelectedObject("StatTable",lPos)
		Set Ndata = obj.GetPrivateNamedData
		NData.SetValue "\StatDependence\ChartParams\Axis\X_Begin",x1
		NData.SetValue "\StatDependence\ChartParams\Axis\X_End",x2
		NData.SetValue "\StatDependence\ChartParams\Axis\Y_Begin",y1
		NData.SetValue "\StatDependence\ChartParams\Axis\Y_End",y2
		set Ndata = Nothing
		Set obj = Nothing
	End If
End Sub

Sub Set_Distr_Scale()
	lPos = 0
	Objects.GetFirstSelectedObjectPos "StatObject", lPos
	Set obj = Objects.GetNextSelectedObject("StatObject",lPos)	
	If Docs.IsPointerNull(obj) Then Exit Sub	
	
	min = 1e+300
	max = 0
	y2 = 0
	sY_param = Data.GetValue("\StatUI\ChartParams\Axis\Y_ParamKey")
	min = global.Min(min,obj.GetValue( -1, 196613))
	max = global.Max(max,obj.GetValue( -1, 196614))
	
	If Data.Getvalue ("\StatUI\ChartParams\ChartType") Then
		For i = 0 To obj.GetClassCount() - 1
			y2 = y2 + obj.GetValue(i, sY_param)
		Next
	else
		For i = 0 To obj.GetClassCount() - 1
			Value = obj.GetValue(i, sY_param)
			If Value > y2 Then y2 = Value
		Next
	End If

	While lPos <> 0
		Set obj = Objects.GetNextSelectedObject("StatObject",lPos)
		min = global.Min(min,obj.GetValue( -1, 196613))
		max = global.Max(max,obj.GetValue( -1, 196614))
		If Data.Getvalue ("\StatUI\ChartParams\ChartType") Then
			For i = 0 To obj.GetClassCount() - 1
				y2 = y2 + obj.GetValue(i, sY_param)
			Next
		else
			For i = 0 To obj.GetClassCount() - 1
				Value = obj.GetValue(i, sY_param)
				If Value > y2 Then y2 = Value
			Next
		End If
	Wend
	If max <= 0 Then exit sub

	iNum = 6
	iStepY = y2 / iNum
	If iStepY >0 Then 
		iStepY = global.RoundEx(iStepY)
	else
		iStepY = 1
	End If
	StepExY = global.Ex(iStepY)

	sOS = global.GetNameStatObj()
	iStepX = (max-min) / iNum
	If iStepX <= 0 Then exit sub
	iStepX = global.RoundEx(iStepX)
	If iStepX = 0 Then iStepX = 1
	StepEx = global.Ex(iStepX)
	If min < iStepX Then
		Data.SetValue "\StatUI\ChartParams\Axis\X_Begin", 0
		global.ObjectSetValue sOS, "\StatUI\ChartParams\Axis\X_Begin", 0
	else
		x1 = 0
		do
			x1 = x1 + iStepX
		loop while x1 < min
		x1 = x1 - iStepX
		str = Application.VtStr(Application.VtDbl(FormatNumber(x1,StepEx)), "%16.16f")
		Data.SetValue "\StatUI\ChartParams\Axis\X_Begin", str
		global.ObjectSetValue sOS, "\StatUI\ChartParams\Axis\X_Begin", str
	End If

	str = FormatNumber(max+1/10^StepEx/2,StepEx)
	str = Application.VtDbl(str)
	str = Application.VtStr(str, "%16.16f")
	Data.SetValue "\StatUI\ChartParams\Axis\X_End", str
	global.ObjectSetValue sOS, "\StatUI\ChartParams\Axis\X_End", str
	Data.SetValue "\StatUI\ChartParams\Axis\X_UseValueRange", 1		' по параметру
	global.ObjectSetValue sOS, "\StatUI\ChartParams\Axis\X_UseValueRange", 1
	Data.SetValue "\StatUI\ChartParams\Axis\X_ScaleFactorValues", iStepX
	global.ObjectSetValue sOS, "\StatUI\ChartParams\Axis\X_ScaleFactorValues", iStepX
	Data.SetValue "\StatUI\ChartParams\Axis\X_ScaleFactor", iStepX
	global.ObjectSetValue sOS, "\StatUI\ChartParams\Axis\X_ScaleFactor", iStepX

'	ось Y 
	Data.SetValue "\StatUI\ChartParams\Axis\Y_ScaleFactor", iStepY / 2
	global.ObjectSetValue sOS, "\StatUI\ChartParams\Axis\Y_ScaleFactor", iStepY / 2

	Data.SetValue "\StatUI\ChartParams\Axis\Y_ScaleFactorGrid", iStepY / 2
	global.ObjectSetValue sOS, "\StatUI\ChartParams\Axis\Y_ScaleFactorGrid", iStepY / 2

	global.ObjectSetValue sOS, "\StatUI\ChartParams\Axis\Y_ScaleFactor", iStepY
	Data.SetValue "\StatUI\ChartParams\Axis\Y_ScaleFactorValues", iStepY
	global.ObjectSetValue sOS, "\StatUI\ChartParams\Axis\Y_ScaleFactorValues", iStepY


	Set Ndata = Nothing
	set obj = Nothing

End Sub

Sub CalcStep()
	Objects.GetFirstSelectedObjectPos "StatTable", lPos
	If lPos = 0 Then Exit Sub
	Set obj = Objects.GetNextSelectedObject("StatTable",lPos)	
	Set Ndata = obj.GetPrivateNamedData
	
	iNum = 6
	iStepX = (x2-x1) / iNum
	If iStepX <= 0 Then exit sub
	iStepX = global.RoundEx(iStepX)
	If iStepX = 0 Then iStepX = 1
	StepEx = global.Ex(iStepX)

	NData.SetValue "\StatDependence\ChartParams\Axis\X_ScaleFactorValues", iStepX
	NData.SetValue "\StatDependence\ChartParams\Axis\X_ScaleFactor", iStepX / 2
	NData.SetValue "\StatDependence\ChartParams\Axis\X_ScaleFactorGrid", iStepX / 2

	NData.SetValue "\StatDependence\ChartParams\Axis\X_UseValueRange", 1
	If x1 < iStepX Then
		X_Begin = 0
	else		
		X_Begin = 0
		do
			X_Begin = X_Begin + iStepX 
		loop while X_Begin < x1
		X_Begin = X_Begin - iStepX 
	End If
	NData.SetValue "\StatDependence\ChartParams\Axis\X_Begin", X_Begin
	NData.SetValue "\StatDependence\ChartParams\Axis\X_End", x2
	
	
	iStepY = (y2-y1) / iNum
	If iStepY <= 0 Then exit sub
	iStepY = global.RoundEx(iStepY)
	If iStepY = 0 Then iStepY = 1
	StepEx = global.Ex(iStepY)
'	NData.SetValue "\StatDependence\ChartParams\Axis\Y_ScaleFactorValues", iStepY
'	NData.SetValue "\StatDependence\ChartParams\Axis\Y_ScaleFactor", iStepY / 2		
'	NData.SetValue "\StatDependence\ChartParams\Axis\Y_ScaleFactorGrid", iStepY / 2

	NData.SetValue "\StatDependence\ChartParams\Axis\Y_UseValueRange", 1
	If y1 < iStepY Then
		Y_Begin = 0
	else		
		Y_Begin = 0
		do
			Y_Begin = Y_Begin + iStepY 
		loop while Y_Begin < y1
		Y_Begin = Y_Begin - iStepY
	End If
	NData.SetValue "\StatDependence\ChartParams\Axis\Y_Begin", Y_Begin
	NData.SetValue "\StatDependence\ChartParams\Axis\Y_End", y2	
	
	Set Ndata = Nothing
	set obj = Nothing

End Sub

