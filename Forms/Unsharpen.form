'***************************************************************
'btnSave event handlers.
'***************************************************************
Sub btnSave_Click()	
	strName = Trim( InputBox(sPresetName, sSavePreset) )
	If strName = "" Then Exit Sub
	If LCase(strName) = LCase(sLastUsed) Then
		MsgBox sNoCorrectName, vbExclamation, gl_Script.sVT
		Exit Sub
	End If

	isOver = False
	n = cmbList.GetStringsCount
	ii = n
	For i = 1 To n - 1
		If strName = cmbList.GetString( i ) Then 
			isOver = True
			ii = i
			Exit For
		End If
	Next
	If isOver Then
		intAnswer = MsgBox(Replace(sOverwrite, "*", strName), vbQuestion + vbYesNo, gl_Script.sVT)
		If intAnswer = vbNo Then Exit Sub
	End If
	If Not isOver Then cmbList.InsertString n, strName
	cmbList.SetSel(ii)
	btnDel.Disabled = CBool(n < 0)

	Data.Setvalue "\Unsharpen\Count", cmbList.GetStringsCount - 1

	i = cmbList.GetSel() - 1
	Data.Setvalue "\Unsharpen\Mask"+CStr(i), sldMask.PosLo
	Data.Setvalue "\Unsharpen\Edge"+CStr(i), sldEdge.PosLo
	Data.Setvalue "\Unsharpen\Ampl"+CStr(i), sldAmpl.PosLo

	SaveStyleNames()
End Sub

'***************************************************************
'btnDel event handlers.
'***************************************************************
Sub btnDel_Click()
	If MsgBox(sDelete, vbQuestion + vbYesNo, gl_Script.sVT)= vbNo Then Exit Sub

	i = cmbList.GetSel()
	For x=i-1 To cmbList.GetStringsCount-3
		a = Data.Getvalue ("\Unsharpen\Mask"+CStr(x+1))
		Data.Setvalue "\Unsharpen\Mask"+CStr(x), a
		
		a = Data.Getvalue ("\Unsharpen\Edge"+CStr(x+1))
		Data.Setvalue "\Unsharpen\Edge"+CStr(x), a

		a = Data.Getvalue ("\Unsharpen\Ampl"+CStr(x+1))	
		Data.Setvalue "\Unsharpen\Ampl"+CStr(x), a
	Next
	
	cmbList.DeleteString(i)	
	cmbList.SetSel -1
	btnDel.Disabled = True

	Data.Setvalue "\Unsharpen\Count", cmbList.GetStringsCount - 1

	SaveStyleNames()
End Sub

Sub SaveStyleNames
	Data.Setvalue "\Unsharpen\Count", cmbList.GetStringsCount - 1
	For i=1 To cmbList.GetStringsCount - 1
		Data.Setvalue "\Unsharpen\Name"+CStr(i-1), cmbList.GetString( i )
	Next
	a = cmbList.GetSel()
	Data.Setvalue "\Unsharpen\Current", a - 1
End Sub

'===============================================================
'
' Zoom
'
'***************************************************************
'sldZoom event handlers.
'***************************************************************
Sub sldZoom_PosChanged()
	Dim frame, s
	Set frame = MainWnd.ActiveFrame
	Set s = frame.GetSplitter	
	
	dZoom = s.GetViewZoom(0,0)
	dNewZoom = Exp(sldZoom.PosLo / 100)
	dz = dNewZoom - dZoom
	cWidth = s.GetViewClientWidth(0,0)
	cHeight = s.GetViewClientHeight(0,0)
	ScrlX = s.GetViewXScroll(0,0)
	ScrlY = s.GetViewYScroll(0,0)
	dX = cWidth * dz * ( ScrlX + s.GetColWidth(0) / 2 ) / ( cWidth * dZoom )
	dY = cHeight * dz * ( ScrlY + s.GetRowHeight(0) / 2 ) / ( cHeight * dZoom )
	s.SetViewZoom 0, 0, dNewZoom
	s.SetViewScroll 0, 0, ScrlX + dX, ScrlY + dY
	edtZoomValue.Text = CStr(Round(dNewZoom * 100, 0)) & "%"
	
	Set s = Nothing
	Set frame = Nothing
End Sub

