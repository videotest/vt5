' ”становка движка по текущему Zoom //
'---------------------------------------------------------------
Sub set_sldZoom
	Dim frame, s
	Set frame = MainWnd.ActiveFrame
	Set s = frame.GetSplitter	
	sldZoom.PosLo = Log(s.GetViewZoom(0,0)) * 100
	edtZoomValue.Text = CStr(Round(s.GetViewZoom(0,0) * 100, 0)) & "%"
	Set s = Nothing
	Set frame = Nothing
End Sub

