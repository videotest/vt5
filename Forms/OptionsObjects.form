[Script]
Dim sInfo, sQuestion, sTitle

Dim strColouring

intCurrentScheme = -1

'***************************************************************
'Form event handlers.
'***************************************************************
Sub Form_OnInitialize()
	sInfo = Docs.LoadString("Information")
	sQuestion = Docs.LoadString("Enter new color set name to save")
	sTitle = Docs.LoadString("New Color Set")

	txtColorsCaption.Text = Docs.LoadString ("Colors:")
	txtShowCaption.Text = Docs.LoadString ("Show:")
	
	lstColors.SetColumnCaption 0, Docs.LoadString ("Name")
	lstColors.SetColumnCaption 1, Docs.LoadString ("Color")
	lstColors.SetCellValue 0, 0, Docs.LoadString ("Object Class")
	lstColors.SetCellValue 0, 1, Docs.LoadString ("Unknown Class")
	
	chkAllNColorsEq.Caption = Docs.LoadString ("The Same Color for All Numbers")
	chkShowClass.Caption = Docs.LoadString ("Object Class")

	If Data.Getvalue ("\ImageView\N_And_N_AsClassColor") = 0 Then chkAllNColorsEq.Checked 1
	If Data.Getvalue ("\ImageView\ShowClassName") Then 
		chkShowClass.Checked 1
		chkAllNColorsEq.Disabled = False
	Else 
		chkAllNColorsEq.Disabled = True
	End If
End Sub

Sub Form_OnOK()
	Data.Setvalue "\ImageView\ShowClassName", Abs(chkShowClass.Checked)
	Data.Setvalue "\ImageView\N_And_N_AsClassColor", Abs(1 - chkAllNColorsEq.Checked)

	clr = Data.Getvalue ("\Classes\UnkClassColor")
	Str = clr + ",?"
	Application.WritePrivateProfileString Data.Getvalue ("\Classes\ClassFile"),"Classes",-1,Str
	Form.UpdateSystemSettings
End Sub


'***************************************************************
'chkShowClass event handlers.
'***************************************************************
Sub chkShowClass_OnCheck(nCheck)
	If chkShowClass.Checked Then 
		chkAllNColorsEq.Disabled = False
	Else 
		chkAllNColorsEq.Disabled = True
	End If
End Sub