sub btnShowSrc_Click()
	Dim frame, s
	Set frame = MainWnd.ActiveFrame
	If TypeName(frame) <> "Nothing" Then
		Set s = frame.GetSplitter
		If btnShowSrc.IsPressed Then
			s.ActivateView 0, 0, "ImageDoc.SrcView"
		Else
			If btnSplit.IsPressed Then
				s.ActivateView 0, 0, "ImageDoc.BinaryFore"	'"ImageDoc.BinaryFore"
			Else
				s.ActivateView 0, 0, "ImageDoc.Phase"	'"ImageDoc.BinaryFore"
			End If
		End If
	End If
	Div()
	Div()
end sub


