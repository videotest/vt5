'включаем правильный Image //
Sub CheckProperImage
	Set objActiveImage = Objects.ActiveImage
	Set objBin = Objects.GetActiveObject("Binary")
	Set objImage = Objects.GetObjectByName( gl_Script.GetParentObjects(objBin) )
	If Docs.IsPointerNull (objImage) Then Exit Sub
	If Docs.IsPointerNull (objBin) Then Exit Sub
	If Not ( objActiveImage Is objImage ) Then Objects.ActiveImage = objImage

	Set objParent = objImage.GetParent()
	If Not Docs.IsPointerNull (objParent) Then	'если нанесена рамка //
		sActiveSelectionName = objImage.Name
		Set objImage = objParent
		Objects.ActiveImage = objParent
	End If

	sBinName = gl_Script.FindSelectedChildObject(objImage, 4)
	If objBin.Name = sBinName Then Exit Sub

	gl_Script.DeSelectAllObjects("Image")
	btnShowSrc.ResetPressedState
	btnShowSrc.Disabled = True
	chkTrans.Disabled = True
End Sub