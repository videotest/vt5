[Script]
Dim iMethod, iAlgorithm
Dim sResultOLName
Dim StateFit2ImageBeforeForm, StateShowPropertyBeforeForm, StateSendToBeforeForm, StateCutDocBySelBeforeForm 

Dim sActiveSelectionName	'название рамки на входе в форму //

Dim strDivType	' Тип разделенеия "Morph" - морфологическое, "Contact" - по длине контакта //

Dim gl_Script
Set gl_Script = Application.GetGlobalScript("")

'***************************************************************
'Form event handlers.
'***************************************************************
sub Form_OnInitialize()
	Data.SetValue "\Form\WasPressOk", 0
	Data.SetValue "\DockBars\PageInProcessing", True

'	ActionManager.HideDockBars 
	
	rdbMethod.DeleteAll
	rdbMethod.AddItem Docs.LoadString ("Long")
	rdbMethod.AddItem Docs.LoadString ("Middle")
	rdbMethod.AddItem Docs.LoadString ("Short")
	rdbMethod.ReCreate
	
	txtCaption.Text = Docs.LoadString ("Automatic Masks Separation")
	btnFitToScreen.ToolTip = Docs.LoadString ("Fit Image to Screen")
	btnZoom0.ToolTip = Docs.LoadString ("Zoom 1:1")
	btnSplit.ToolTip = Docs.LoadString ("Split Window")
	btnShowSrc.ToolTip = Docs.LoadString ("Source Image")
	txtZoom.Text = Docs.LoadString ("Zoom")
	txtDivideTypeCaption.Text = Docs.LoadString ("Distribution\nof objects masks:")
	btnDivideMorph.ButtonText = Docs.LoadString ("Rare")
	btnDivideContact.ButtonText = Docs.LoadString ("Compact")
	txtNoOptions.Text = Docs.LoadString ("No settings\nfor this\ndistribution")
	chkTrans.Caption = Docs.LoadString ("Semitransparent masks")
	txtMethodCaption.Text = Docs.LoadString ("Contact Length")

	'Название списка объектов, создаваемого по ОК (возможно не требуется) //
	Set ObjList = Objects.GetActiveObject( "Object List" )
	If not Docs.IsPointerNull(ObjList) Then
		sResultOLName = ObjList.name
	else
		sResultOLName = "Object List"
	End If
	Set ObjList = Nothing
	
	Form.InitializePreview

	'ShowBinaryFore
	Call CheckProperImage
	ShowPhase

	rdbMethod.ItemChecked Data.Getvalue ("\MorphDivide\Method")

	'Если форма не показывется, не делим окно //
	If Data.GetValue ("\FormManager\DisableFormShow") = 0 Then
		If Data.Getvalue ("\MorphDivide\SplitWindow") Then btnSplit.SetPressedState
		If btnShowSrc.Disabled = 0 Then _
		  If Data.GetDefValue ("\MorphDivide\ShowSrc", 0) Then btnShowSrc.SetPressedState
	End If
	Call btnSplit_Click()
	Call btnFitToScreen_Click

	chkTrans.Checked Data.Getvalue ("\MorphDivide\Transparent")			'полупрозрачные маски //
	Call chkTrans_OnCheck(chkTrans.Checked)
	
	'Call btnShowSrc_Click()

	strDivType = Data.GetDefValue("\MorphDivide\DivType", "Morph")

	Div()  

	'Блокируем команды в контекстном меню //
	StateFit2ImageBeforeForm = ActionState.GetActionState ("Fit2Image")
	StateShowPropertyBeforeForm = ActionState.GetActionState ("ShowPropertyBar")
	StateSendToBeforeForm = ActionState.GetActionState ("SendTo")
	StateCutDocBySelBeforeForm = ActionState.GetActionState ("CutDocBySel")
	
	If StateFit2ImageBeforeForm Then ActionState.SetActionState "Fit2Image", 0
	If StateShowPropertyBeforeForm Then ActionState.SetActionState "ShowPropertyBar", 0
	If StateSendToBeforeForm Then ActionState.SetActionState "SendTo", 0
	If StateCutDocBySelBeforeForm Then ActionState.SetActionState "CutDocBySel", 0
end sub

Sub Form_OnChangePreview()
	If bSplitInProcessing Then Exit Sub
'	Div()	'отсутствие ошибки при нажатии btnShowSrc в неразделенном окне //
'	ActionState.TerminateCurrentAction
End Sub

sub Form_OnOK()
	Form.ApplyPreview

	If Not Docs.IsPointerNull(MethodMan) Then MethodMan.StartSetValueLogging

	Data.SetValue "\MorphDivide\DivType", strDivType
	Data.Setvalue "\MorphDivide\Method",rdbMethod.ItemChecked  
'	MeasureBin "","",1, sResultOLName
'	ShowImage
	ShowBinaryFore

	'Не запоминаем деление окна если форма не показывется //
	If Data.GetValue ("\FormManager\DisableFormShow") = 0 Then 
		Data.SetValue "\MorphDivide\SplitWindow", btnSplit.IsPressed
		Data.SetValue "\MorphDivide\ShowSrc", btnShowSrc.IsPressed
	End If

	If Not Docs.IsPointerNull(MethodMan) Then MethodMan.FinishSetValueLogging

	Data.SetValue "\Form\WasPressOk", 1
end sub

sub Form_OnCancel()
	Form.CancelPreview
end sub

sub Form_OnDestroy()
	'btnSplit.ResetPressedState
	'Call btnSplit_Click()

	If sActiveSelectionName <> "" Then ObjectSelect sActiveSelectionName

	Data.SetValue "\DockBars\PageInProcessing", False
'	ActionManager.ShowDockBars

	Set gl_Script = Nothing

	'Разблокируем команды в контекстном меню //
	If StateFit2ImageBeforeForm Then ActionState.ResetActionState "Fit2Image"
	If StateShowPropertyBeforeForm Then ActionState.ResetActionState "ShowPropertyBar"
	If StateSendToBeforeForm Then ActionState.ResetActionState "SendTo"
	If StateCutDocBySelBeforeForm Then ActionState.ResetActionState "CutDocBySel"
end sub

'===============================================================
'===============================================================
Sub Div()
	Select Case strDivType
	    Case "Contact"
			btnDivideMorph.ResetPressedState
			btnDivideContact.SetPressedState
			txtMethodCaption.Visible = True
			rdbMethod.Visible = True
			txtNoOptions.Visible = False
			ActionHelper.MorphDivideExBin rdbMethod.ItemChecked+1 , 1
	    Case Else
			btnDivideMorph.SetPressedState
			btnDivideContact.ResetPressedState
			txtMethodCaption.Visible = False
			rdbMethod.Visible = False
			txtNoOptions.Visible = True
			ActionHelper.BinMorphDivision
	End Select
End Sub

