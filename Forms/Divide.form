	End If
'	ActionState.TerminateCurrentAction
'	Application.ProcessMessage 0
	UpdateWindow 1
end sub

'***************************************************************
'btnSplit event handlers.
'***************************************************************
Sub btnSplit_Click()
	bSplitInProcessing = True
	Dim frame, s
	Set frame = MainWnd.ActiveFrame
	If TypeName(frame) <> "Nothing" Then
		Set s = frame.GetSplitter	
		If btnSplit.IsPressed Then
			s.Split 2, 1
			If btnShowSrc.IsPressed Then
				s.ActivateView 0, 0, "ImageDoc.SrcView"
			Else
				s.ActivateView 0, 0, "ImageDoc.BinaryFore"	'"ImageDoc.BinaryFore"
			End If
			s.ActivateView 1, 0, "ImageDoc.Phase"	'"ImageDoc.BinaryFore"

			SyncZoom 1
		Else
			btnShowSrc.ResetPressedState
			s.Split 1, 1
			s.ActivateView 0, 0, "ImageDoc.Phase"

			If btnShowSrc.IsPressed Then btnShowSrc.ResetPressedState
		End If
		Set s = Nothing
	End If
	Set frame = Nothing
	Div()
	Div()
	bSplitInProcessing = False
End Sub

