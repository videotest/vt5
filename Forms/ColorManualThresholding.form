Sub phases_initialise
	NulBackColorsCount = 1

	FindPhasesColor NulBackColorsCount, 0

	colPh.RemoveAll
	For i = 0 To NulBackColorsCount - 1
		iPhaseN = i
		colPh.Add i, ""
		Set colPh.item(i) = New ColorPhase
'		colPh.item(i).Color = Data.GetDefValue("\SegmColorPhases\Color" & CStr(i), "(0,0,0)")
		colPh.item(i).Color = "(-1,-1,-1)"
		colPh.item(i).Selected = False
	Next

	Data.SetValue "\Phase\PhasesCount", colPh.Count
	Data.SetValue "\SegmColorPhases\Count", colPh.Count

	Call write_weights
End Sub

