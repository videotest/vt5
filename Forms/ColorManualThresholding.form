'---------------------------------------------------------------
'  True,    //
'---------------------------------------------------------------
Function is_equal_colors(ByVal sColor1, ByVal sColor2)
	sColor1 = Replace(sColor1, " ", "")
	sColor2 = Replace(sColor2, " ", "")
	is_equal_colors = ( sColor1 = sColor2 )
End Function

'---------------------------------------------------------------
'  True,     RGB    30 //
'---------------------------------------------------------------
Function is_near_colors(sColor1, sColor2)
	Dim arrColor1, arrColor2, i, dif
	is_near_colors = False
	arrColor1 = get_color_components(sColor1)
	arrColor2 = get_color_components(sColor2)
	If UBound(arrColor1) <> 2 Or UBound(arrColor2) <> 2 Then Exit Function
	dif = 0
	For i = 0 To 2
		dif = dif + Abs( arrColor1(i) - arrColor2(i) )
	Next
	If dif < 30 Then is_near_colors = True
End Function

'---------------------------------------------------------------
'   "(125, 255,  0)" -> array(125,255,0) //
'---------------------------------------------------------------
Function get_color_components(ByVal sValue)
	Dim colors, i
	sValue = Replace(sValue, "(", "")
	sValue = Replace(sValue, ")", "")
	sValue = Replace(sValue, " ", "")
	colors = Split(sValue, ",")
	For i = 0 To UBound(colors)
		colors(i) = CInt(colors(i))
	Next
	get_color_components = colors
End Function

'---------------------------------------------------------------
'    //
'---------------------------------------------------------------
Function get_average_color(sColor1, sColor2)
	Dim arrColor1, arrColor2, i
	get_average_color = sColor1
	arrColor1 = get_color_components(sColor1)
	arrColor2 = get_color_components(sColor2)
	If UBound(arrColor1) <> 2 Or UBound(arrColor2) <> 2 Then Exit Function
	For i = 0 To 2
		arrColor1(i) = Int( ( arrColor1(i) + arrColor2(i) ) / 2 )
	Next
	get_average_color = "(" & Join(arrColor1, ",") & ")"
End Function

'==============================================================
'       (r,g,b) //
' arrPhases -     //
' sGivenColor -   //
'      //
' FindNearColor = -1     -  //
'==============================================================
Function FindNearPhases(arrPhases, iStartFrom)
	Dim best(1)
	Dim sColor, arrPanes(), arrPanes1, i, j
	Redim arrPanes(Ubound(arrPhases), 2)

	best(0) = -1
	best(1) = -1
	FindNearPhases = best

	For i = iStartFrom To UBound(arrPhases)
		sColor = Replace(arrPhases(i).Color, " ", "")
		sColor = Replace(sColor, "(", "")
		sColor = Replace(sColor, ")", "")
		arrPanes1 = Split(sColor, ",")
		If UBound(arrPanes1) <> 2 Then Exit Function
		For nPane = 0 To 2
			arrPanes(i, nPane) = arrPanes1(nPane)
		Next
	Next

	r_best = 1e20
	For i = iStartFrom To UBound(arrPhases) - 1
		j = i + 1

		Do While j <= UBound(arrPhases)
			
			'        //
			If arrPhases(i).Selected = arrPhases(j).Selected Then

				r = 0
				For nPane = 0 To 2
					r = r + ( CLng( arrPanes(j, nPane) ) - CLng( arrPanes(i, nPane) ) ) ^ 2
				Next

				'    //
				p = 0
				r1 = 0
				r2 = 0
				For nPane = 0 To 2
					r1 = r1 + CLng( arrPanes(i, nPane) ) ^ 2
					r2 = r2 + CLng( arrPanes(j, nPane) ) ^ 2
					p = p + CLng( arrPanes(i, nPane) ) * CLng( arrPanes(j, nPane) )
				Next
				delta = r1 - p * p / ( r2 + 1e-20 )

				r = r * 0.1 + delta * 0.9
				'end //

				If r < r_best Then
					best(0) = i
					best(1) = j
					r_best = r
				End If

			End If

			j = j + 1
		Loop
	Next
	FindNearPhases = best
End Function

