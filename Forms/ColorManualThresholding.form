Sub write_phases_info
	Dim fso
	Set fso = CreateObject("Scripting.FileSystemObject")
	strFile = Data.GetValue("\Paths\CurrentPath") & "classes_info.txt"
	Set f = fso.CreateTextFile( strFile, True )

	For i = 0 To colPh.Count - 1
		sLine = Right(" " & CStr(i), 2) & ". " & colPh.item(i).Color
		If colPh.item(i).Selected Then sLine = sLine & "  1"
		f.WriteLine sLine
	Next
	f.Close
	Set fso = Nothing
End Sub

