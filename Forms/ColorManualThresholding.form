	For i = 0 To colPh.Count - 1
		sI = CStr(i)
		SetFormSettings "Phases\Phase" & sI & "Color", colPh.item(i).Color
		SetFormSettings "Phases\Phase" & sI & "Selected", colPh.item(i).Selected
	Next
	SetFormSettings "Phases\PhasesCount", colPh.Count
	SetFormSettings "Phases\NulBackColorsCount", NulBackColorsCount
	SetFormSettings "Weight", sldWeight.PosLo

	'TerminateCurrentAction -    strPrimaryBin //
	ActionState.TerminateCurrentAction
	If strPrimaryBin <> "" Then ObjectDelete strPrimaryBin

'	Form.CancelPreview

	SegmColorPhases "", GetFormSettings("BinName", "")

	iEndUndoMode = 1

	'        //
