Sub PhasesChanged()	
	Call update_undo_list
	Call UpdatePreview
End Sub

'---------------------------------------------------------------
'       //
'---------------------------------------------------------------
Sub UpdatePreview
	Call Refresh
	Call fill_lst
End Sub

'---------------------------------------------------------------
'   //
'---------------------------------------------------------------
Sub Refresh
	If chkShowSelected.Checked Then
		For i = 0 To colPh.Count - 1
			'MsgBox "colPh.item(i).Selected = " & colPh.item(i).Selected
			If colPh.item(i).Selected Then
				Data.SetValue "\SegmColorPhases\Phase" & CStr(i), 255
			Else
				Data.SetValue "\SegmColorPhases\Phase" & CStr(i), 0
			End If
		Next
	Else
		For i = 0 To colPh.Count - 1
			Data.SetValue "\SegmColorPhases\Phase" & CStr(i), 128 + i
		Next
	End If
	ActionHelper.SegmColorPhases
End Sub


'---------------------------------------------------------------
'  -    //
'---------------------------------------------------------------
Sub fill_lst
	For i = 0 To lstPhases.GetRowCount - 1
		lstPhases.DeleteRow 0
	Next

	For i = 0 To colPh.Count - 1
		iPhaseN = i
		lstPhases.AddRow i
		lstPhases.SetCellValue 0, i, i
		lstPhases.SetCellAsLogical 1, i, Abs(colPh.item(i).Selected)
		lstPhases.SetCellValueColorRGB 2, i, colPh.item(i).Color
	Next

	txtBg.Text = NulBackColorsCount
	txtTotal.Text = colPh.Count
End Sub


