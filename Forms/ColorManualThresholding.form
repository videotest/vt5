Sub create_phases_fromSD
	colPh.RemoveAll
	For i = 0 To GetFormSettings("Phases\PhasesCount", 0) - 1
		iPhaseN = i
		colPh.Add i, ""
		Set colPh.item(i) = New ColorPhase
		sI = CStr(i)
		colPh.item(i).Color = _
		  GetFormSettings("Phases\Phase" & sI & "Color", "(0,0,0)")
		colPh.item(i).Selected = _
		  GetFormSettings("Phases\Phase" & sI & "Selected", 0)
	Next

	NulBackColorsCount = GetFormSettings("Phases\NulBackColorsCount", 0)

	Data.SetValue "\Phase\PhasesCount", colPh.Count
	Data.SetValue "\SegmColorPhases\Count", colPh.Count

	Call write_weights
End Sub

