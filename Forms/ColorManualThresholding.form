		Data.SetValue "\Phase\Phase" & CStr(iPhaseN + 1) & "Color", vData
		Data.SetValue "\SegmColorPhases\Color" & CStr(iPhaseN), vData
	End Property

	Public Property Get Color()
		Color = mvarColor
	End Property

