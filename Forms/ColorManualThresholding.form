Sub btnUndo_Click()
	colUndo.Remove UndoPos
	UndoPos = UndoPos - 1
	If UndoPos <= 0 Then btnUndo.Disabled = True
	bSkipUndo = True
	n = colUndo.Item(UndoPos).Count

	Data.SetValue "\Phase\PhasesCount", n
	Data.SetValue "\SegmColorPhases\Count", n

	For i = 0 To n - 1
		iPhaseN = i		'   .      ColorPhase //
		If colPh.Count < (i + 1) Then
			colPh.Add i, ""
			Set colPh.item(i) = New ColorPhase
		End If
		With colUndo.Item(UndoPos).Item(i)
			colPh.Item(i).Color = .Color
			colPh.Item(i).Selected = .Selected
		End With
	Next

	If lstPhases.GetRowCount > n Then
		lstPhases.DeleteRow n
		colPh.Remove n
	End If

	Call write_weights
	Call UpdatePreview
	bSkipUndo = False
End Sub

'---------------------------------------------------------------
'     Undo //
'---------------------------------------------------------------
Sub update_undo_list
	If bSkipUndo Then Exit Sub
	UndoPos = UndoPos + 1
	If UndoPos > 0 Then btnUndo.Disabled = False

	Set colUndo.Item(UndoPos) = CreateObject("Scripting.Dictionary")
	For i = 0 To colPh.Count - 1
'		colUndo.Item(colUndo.Count - 1).Add i, ""
		Set colUndo.Item(UndoPos).Item(i) = New UndoItem
		With colUndo.Item(UndoPos).Item(i)
			.Color = colPh.Item(i).Color
			.Selected = colPh.Item(i).Selected
		End With
	Next
	'MsgBox "update_undo_list"
End Sub


