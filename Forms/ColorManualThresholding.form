sub chkTrans_OnCheck(nCheck)
	Dim bTemp
	If nCheck Then
		Data.SetValue "\Phase\Transparency", 80
	Else
		Data.SetValue "\Phase\Transparency", 0
	End If
	bTemp = bSkipUndo
	bSkipUndo = True
	ActionState.TerminateCurrentAction
	Call PhasesChanged
	bSkipUndo = bTemp
end sub

