sub sldWeight_PosChanged()
	Call write_weights
	Call Refresh
end sub

'---------------------------------------------------------------
'    //
'---------------------------------------------------------------
Sub write_weights
	dWeightObjects = Exp(sldWeight.PosLo / 170)
	dWeightBack = Exp(-sldWeight.PosLo / 170)
	For i = 1 To colPh.Count - 1
		If colPh.item(i).Selected Then
			Data.SetValue "\SegmColorPhases\Weight" & CStr(i), dWeightObjects
		Else
			Data.SetValue "\SegmColorPhases\Weight" & CStr(i), dWeightBack
		End If
	Next
	Data.SetValue "\SegmColorPhases\Weight0", 5
End Sub

