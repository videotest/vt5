sub optMedian_OnChangeCheck(lItem)
	cmbList.SetSel -1
	btnDel.Disabled = True

	strFunc = "Median"
	Call RunFunc()
end sub

sub optMeanEV_OnChangeCheck(lItem)
	cmbList.SetSel -1
	btnDel.Disabled = True

	strFunc = "MeanEV"
	Call RunFunc()
end sub

sub optMeanKNV_OnChangeCheck(lItem)
	cmbList.SetSel -1
	btnDel.Disabled = True

	strFunc = "MeanKNV"
	Call RunFunc()
end sub

'***************************************************************
'edtMask event handlers.
