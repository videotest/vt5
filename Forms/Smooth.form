Sub Form_OnInitialize()
	Set gl_Script = Application.GetGlobalScript("")
	Data.SetValue "\Form\WasPressOk", 0

	sPresetName = Docs.LoadString ("Name:")
	sSavePreset = Docs.LoadString ("Save User Settings")
	sOverwrite = Docs.LoadString ("Settings named ""*"" already exist.\n Overwrite?")
	sLastUsed = Docs.LoadString ("Last Used")
	sDelete = Docs.LoadString ("Delete?")
	sBrightCaption = Docs.LoadString ("Gray")
	sNoCorrectName = Docs.LoadString ("This name can not be used!")

	txtSmooth.Text = Docs.LoadString ("Smooth")
	edtMask.CaptionText = Docs.LoadString ("Radius:")
	txtSettings2.Text = Docs.LoadString ("Settings")
	btnSave.ButtonText = Docs.LoadString ("Save")
	btnSave.ToolTip = Docs.LoadString ("Save User Settings")
	btnDel.ButtonText = Docs.LoadString ("Delete")
	btnDel.ToolTip = Docs.LoadString ("Delete")
	sUnCorrectStyle = Docs.LoadString ("This setting uses an inaccessible filter!")

	txtZoom.Text = Docs.LoadString ("Zoom")
	btnSplit.ToolTip = Docs.LoadString ("Split Window")
	btnFitToScreen.ToolTip = Docs.LoadString ("Fit Image to Screen")
	btnZoom0.ToolTip = Docs.LoadString ("Zoom 1:1")

	optMedian.DeleteAll
	optMedian.AddItem Docs.LoadString ("Median")
	optMedian.ReCreate

	optMeanEV.DeleteAll
	optMeanEV.AddItem Docs.LoadString ("By Brightness")
	optMeanEV.ReCreate

	optMeanKNV.DeleteAll
	optMeanKNV.AddItem Docs.LoadString ("By Size")
	optMeanKNV.ReCreate

	If Not Docs.IsPointerNull(MethodMan) Then
		If MethodMan.IsRunning() Then
			optMedian.Enabled = False
			optMeanEV.Enabled = False
			optMeanKNV.Enabled = False
		End If
	End If

	ActionManager.SetBrightnessPanes()
	
	ShowSource
	
	Form.InitializePreview
'	sldMask.PosLo = Data.Getvalue ("\Smooth\Mask") 
'	sld2.PosLo = Data.Getvalue ("\Smooth\sld2")
'	edt2.Text = sld2.PosLo
'	edtMask.Text = sldMask.PosLo
'	If Data.Getvalue ("\Smooth\Fast") Then chkFast.Checked(True)
	ActionState.SetActionState "RunSmooth", 3
	'MsgBox strFunc
	InitStyle()

'	RunFunc()	'предотвращаем мелькание контролов на старте //

	Call set_sldZoom

	Data.SetValue "\DockBars\PageInProcessing", True
	ActionManager.HideDockBars
	'прорисовываем форму //
	Application.ProcessMessage 0

	bNotRunFunc = False

	'≈сли форма не показыветс€, не делим окно //
	If Data.GetValue ("\FormManager\DisableFormShow") = 0 Then
		If Data.GetDefValue ("\Smooth\SplitWindow", 0) Then btnSplit.SetPressedState
	End If
	Call btnSplit_Click()
	Call btnFitToScreen_Click

	RunFuncHist(-1)

	'Ѕлокируем команды в контекстном меню //
	StateFit2ImageBeforeForm = ActionState.GetActionState ("Fit2Image")
	StateShowPropertyBeforeForm = ActionState.GetActionState ("ShowPropertyBar")
	StateSendToBeforeForm = ActionState.GetActionState ("SendTo")
	StateCutDocBySelBeforeForm = ActionState.GetActionState ("CutDocBySel")
	
	If StateFit2ImageBeforeForm Then ActionState.SetActionState "Fit2Image", 0
	If StateShowPropertyBeforeForm Then ActionState.SetActionState "ShowPropertyBar", 0
	If StateSendToBeforeForm Then ActionState.SetActionState "SendTo", 0
	If StateCutDocBySelBeforeForm Then ActionState.SetActionState "CutDocBySel", 0

	bSkipEvents = False
End Sub

Sub Form_OnOK()	
	rec = 1
	ActionState.BeginGroupUndo "vt"
	Form.ApplyPreview	
	ActionManager.SelectMakeVirtual
	ActionState.EndGroupUndo 1
	rec = 0

	If Not Docs.IsPointerNull(MethodMan) Then MethodMan.StartSetValueLogging

	Data.Setvalue "\Smooth\Fast", chkFast.Checked 

	Data.Setvalue "\"+StyleName+"\"+StyleName1+"-1", sldMask.PosLo
	Data.Setvalue "\"+StyleName+"\"+StyleName2+"-1", sld2.PosLo
	Data.Setvalue "\"+StyleName+"\"+StyleName3+"-1", strFunc			'!!!
	Data.Setvalue "\"+StyleName+"\Current", cmbList.GetSel() - 1

	'«апись правильного названи€ выбранной функции //
	If TypeName (MethodMan) <> "Nothing" Then
		If MethodMan.IsRecording() Then
			sStepName = txtSmooth.Text & " " & LCase( Eval( "opt" & strFunc & ".GetItem(0)" ) )
			MethodMan.RenameCurrentStep sStepName
		End If
	End If

	'Ќе запоминаем деление окна если форма не показыветс€ //
	If Data.GetValue ("\FormManager\DisableFormShow") = 0 Then 
		Data.SetValue "\Smooth\SplitWindow", btnSplit.IsPressed
	End If

	If Not Docs.IsPointerNull(MethodMan) Then MethodMan.FinishSetValueLogging

	Data.SetValue "\Form\WasPressOk", 1
End Sub
