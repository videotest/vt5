Sub InitStyle
	cmbList.DeleteContents
	cmbList.InsertString 0, sLastUsed
	n = Data.GetDefValue ("\"+StyleName+"\Count", 0)
	'MsgBox n
	For i=0 To n-1
		Stroka = Data.Getvalue ("\"+StyleName+"\Name"+CStr(i))
		'MsgBox 	Stroka
		cmbList.InsertString i+1, Stroka 
	Next
	a = Data.GetDefValue ("\"+StyleName+"\Current", 0) + 1
	If a < 0 Then a = 0
	If a > n Then a = n
	cmbList.SetSel( a )
	cmbList_OnSelectChange()
End Sub

Sub SaveStyle
	Data.Setvalue "\"+StyleName+"\Count", cmbList.GetStringsCount - 1
	For i=1 To cmbList.GetStringsCount-1
		Data.Setvalue "\"+StyleName+"\Name"+CStr(i-1), cmbList.GetString( i )
	Next
	a = cmbList.GetSel()
	Data.Setvalue "\"+StyleName+"\Current", a - 1
End Sub

Sub cmbList_OnSelectChange()
	n = cmbList.GetSel() - 1
	strNewFunc = Data.Getvalue ("\"+StyleName+"\"+StyleName3+CStr(n))

	If strNewFunc <> "Median" And strNewFunc <> "MeanEV" And strNewFunc <> "MeanKNV" Then _
	  strNewFunc = "Median"
	If strFunc = "" Then
		Execute "opt" & strNewFunc & ".Enabled = True"
		strFunc	= strNewFunc
	End If
	
	
	If Not Docs.IsPointerNull(MethodMan) Then
		If MethodMan.IsRunning() Then
			If strNewFunc <> strFunc Then
				cmbList.SetSel n
				MsgBox sUnCorrectStyle, vbExclamation, gl_Script.sVT
				Exit Sub
			End If
		End If
	End If
	btnDel.Disabled = CBool(n < 0)

	sldMask.PosLo = Data.Getvalue ("\"+StyleName+"\"+StyleName1+CStr(n))	'!!!
	edtMask.Text = sldMask.PosLo
	sld2.PosLo = Data.Getvalue ("\"+StyleName+"\"+StyleName2+CStr(n))		'!!!
	edt2.Text = sld2.PosLo
	strFunc	= strNewFunc		'!!!
	RunFunc()
End Sub

Sub btnSave_Click()
	strName = Trim( InputBox(sPresetName, sSavePreset) )
	If strName = "" Then Exit Sub
	If LCase(strName) = LCase(sLastUsed) Then
		MsgBox sNoCorrectName, vbExclamation, gl_Script.sVT
		Exit Sub
	End If

	isOver = False
	n = cmbList.GetStringsCount
	ii = n
	For i = 1 To n - 1
		If strName = cmbList.GetString( i ) Then 
			isOver = True
			ii = i
			Exit For
		End If
	Next
	If isOver Then
		intAnswer = MsgBox(Replace(sOverwrite, "*", strName), vbQuestion + vbYesNo, gl_Script.sVT)
		If intAnswer = vbNo Then Exit Sub
	End If
	If Not isOver Then cmbList.InsertString n, strName
	cmbList.SetSel(ii)
	btnDel.Disabled = CBool(n < 0)

	Data.Setvalue "\"+StyleName+"\Count", cmbList.GetStringsCount - 1

	i = cmbList.GetSel() - 1
	Data.Setvalue "\"+StyleName+"\"+StyleName1+CStr(i), sldMask.PosLo 	'!!!
	Data.Setvalue "\"+StyleName+"\"+StyleName2+CStr(i), sld2.PosLo		'!!!
	Data.Setvalue "\"+StyleName+"\"+StyleName3+CStr(i), strFunc			'!!!
	SaveStyle()
End Sub

'***************************************************************
'btnDel event handlers.
'***************************************************************
Sub btnDel_Click()
	If MsgBox(sDelete, vbQuestion + vbYesNo, gl_Script.sVT)= vbNo Then Exit Sub

	i = cmbList.GetSel()
	For x=i-1 To cmbList.GetStringsCount-3
		a = Data.Getvalue ("\"+StyleName+"\"+StyleName1+CStr(x+1))
		Data.Setvalue "\"+StyleName+"\"+StyleName1+CStr(x), a		
		a = Data.Getvalue ("\"+StyleName+"\"+StyleName2+CStr(x+1))
		Data.Setvalue "\"+StyleName+"\"+StyleName2+CStr(x), a
		a = Data.Getvalue ("\"+StyleName+"\"+StyleName3+CStr(x+1))
		Data.Setvalue "\"+StyleName+"\"+StyleName3+CStr(x), a
	Next	

	cmbList.DeleteString(i)	
	cmbList.SetSel -1
	btnDel.Disabled = True

	Data.Setvalue "\"+StyleName+"\Count", cmbList.GetStringsCount - 1
	SaveStyle()
End Sub

'===============================================================
'
' Zoom
'
'***************************************************************
'sldZoom event handlers.
'***************************************************************
Sub sldZoom_PosChanged()
	Dim frame, s
	Set frame = MainWnd.ActiveFrame
	Set s = frame.GetSplitter	
	
	dZoom = s.GetViewZoom(0,0)
	dNewZoom = Exp(sldZoom.PosLo / 100)
	dz = dNewZoom - dZoom
	cWidth = s.GetViewClientWidth(0,0)
	cHeight = s.GetViewClientHeight(0,0)
	ScrlX = s.GetViewXScroll(0,0)
	ScrlY = s.GetViewYScroll(0,0)
	dX = cWidth * dz * ( ScrlX + s.GetColWidth(0) / 2 ) / ( cWidth * dZoom )
	dY = cHeight * dz * ( ScrlY + s.GetRowHeight(0) / 2 ) / ( cHeight * dZoom )
	s.SetViewZoom 0, 0, dNewZoom
	s.SetViewScroll 0, 0, ScrlX + dX, ScrlY + dY
	edtZoomValue.Text = CStr(Round(dNewZoom * 100, 0)) & "%"
	
	Set s = Nothing
	Set frame = Nothing
End Sub

