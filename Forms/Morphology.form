bSkipEvents = True

Sub RunFuncHist(P)
	Select Case P
	    Case 0
			btn1.SetPressedState
			btn2.ResetPressedState
			btn3.ResetPressedState
			btn4.ResetPressedState
			btnAll.ResetPressedState
			Hist.SetPane(0)
	    Case 1
			btn1.ResetPressedState
			btn2.SetPressedState
			btn3.ResetPressedState
			btn4.ResetPressedState
			btnAll.ResetPressedState
			Hist.SetPane(1)
	    Case 2
			btn1.ResetPressedState
			btn2.ResetPressedState
			btn3.SetPressedState
			btn4.ResetPressedState
			btnAll.ResetPressedState
			Hist.SetPane(2)
	    Case 3
			btn1.ResetPressedState
			btn2.ResetPressedState
			btn3.ResetPressedState
			btn4.SetPressedState
			btnAll.ResetPressedState
			Hist.SetPane(3)
	    Case -1
			btnAll.SetPressedState
			btn1.ResetPressedState
			btn2.ResetPressedState
			btn3.ResetPressedState
			btn4.ResetPressedState
			Hist.AddPane(0)
			Hist.AddPane(1)
			Hist.AddPane(2)
	End Select
End Sub

Sub RunFunc()
	'ActionState.TerminateCurrentAction	'не работает!
	edt1.CaptionText = sValue
	bReconstruct = CBool(chkReconstruct.Checked)

	optErode.ItemChecked -1
	optDilate.ItemChecked -1
	optOpen.ItemChecked -1
	optClose.ItemChecked -1

	Select Case iFunc
		Case f_Erode
			optErode.ItemChecked 0
			chkReconstruct.Disabled = True
			sld1.Max = 30
			edt1.MaxValue = sld1.Max
			txtMax1.Text = CStr(sld1.Max)
			If bInit Then ActionHelper.Erode iNewValue
		Case f_Dilate
			optDilate.ItemChecked 0
			chkReconstruct.Disabled = True
			sld1.Max = 30
			edt1.MaxValue = sld1.Max
			txtMax1.Text = CStr(sld1.Max)
			If bInit Then ActionHelper.Dilate iNewValue
		Case f_Open
			optOpen.ItemChecked 0
			chkReconstruct.Disabled = False
			sld1.Max = 30
			edt1.MaxValue = sld1.Max
			txtMax1.Text = CStr(sld1.Max)
			If bInit Then
				If Not bReconstruct Then
					ActionHelper.Open iNewValue
				Else
					'Для сравнения //
					'ActionHelper.OpenByReconstruction iNewValue, 1
					Form.BeginFilterGroup
						strImg = Objects.ActiveImage.Name
						DeleteFilterArgsOld = Data.GetValue ("\General\DeleteFilterArgs")
						Data.SetValue "\General\DeleteFilterArgs", 0
						ActionHelper.Erode iNewValue, strImg, "_tmp_Virt_Image"
						Data.SetValue "\General\DeleteFilterArgs", DeleteFilterArgsOld
						ActionHelper.OpenByReconstructionAAM "_tmp_Virt_Image", strImg, strImg
					Form.EndFilterGroup
					'ObjectDelete "_tmp_Virt_Image"
				End If
			End If
		Case f_Close
			optClose.ItemChecked 0
			chkReconstruct.Disabled = False
			sld1.Max = 30
			edt1.MaxValue = sld1.Max
			txtMax1.Text = CStr(sld1.Max)
			If bInit Then
				If Not bReconstruct Then
					If bInit Then ActionHelper.Close iNewValue
				Else
					'Для сравнения //
					'ActionHelper.CloseByReconstruction iNewValue, 1
					Form.BeginFilterGroup
						strImg = Objects.ActiveImage.Name
						DeleteFilterArgsOld = Data.GetValue ("\General\DeleteFilterArgs")
						Data.SetValue "\General\DeleteFilterArgs", 0
						ActionHelper.Dilate iNewValue, strImg, "_tmp_Virt_Image"
						Data.SetValue "\General\DeleteFilterArgs", DeleteFilterArgsOld
						ActionHelper.CloseByReconstructionAAM "_tmp_Virt_Image", strImg, strImg
					Form.EndFilterGroup
					'ObjectDelete "_tmp_Virt_Image"
				End If
			End If
'		Case f_OpenRec
'			sld1.Max = 10
'			edt1.MaxValue = sld1.Max
'			txtMax1.Text = CStr(sld1.Max)
''			Form.BeginFilterGroup
'			ActionHelper.OpenByReconstruction iNewValue, 1
''			Form.EndFilterGroup
'		Case f_CloseRec
'			sld1.Max = 10
'			edt1.MaxValue = sld1.Max
'			txtMax1.Text = CStr(sld1.Max)
'			ActionHelper.CloseByReconstruction iNewValue, 1
	End Select
	sld1.PosLo = iNewValue
	edt1.ValueInt = iNewValue
End Sub

'***************************************************************
'Form event handlers.
'***************************************************************
Sub Form_OnInitialize()
	Data.SetValue "\Form\WasPressOk", 0
	sValue = Docs.LoadString ("Number of \noperations:")

	bSkipEvents = True

	optErode.DeleteAll
	optErode.AddItem Docs.LoadString ("Erode")
	optErode.ReCreate

	optDilate.DeleteAll
	optDilate.AddItem Docs.LoadString ("Dilate")
	optDilate.ReCreate

	optOpen.DeleteAll
	optOpen.AddItem Docs.LoadString ("Open")
	optOpen.ReCreate

	optClose.DeleteAll
	optClose.AddItem Docs.LoadString ("Close")
	optClose.ReCreate

	If Not Docs.IsPointerNull(MethodMan) Then
		If MethodMan.IsRunning() Then
			optErode.Enabled = False
			optDilate.Enabled = False
			optOpen.Enabled = False
			optClose.Enabled = False
		End If
	End If

	sBrightCaption = Docs.LoadString ("Gray")

	txtCaption.Text = Docs.LoadString ("Morphological Filters")
	edt1.CaptionText = Docs.LoadString ("Mask:")
	chkReconstruct.Caption = Docs.LoadString ("With reconstruction")

	txtZoom.Text = Docs.LoadString ("Zoom")
	btnSplit.ToolTip = Docs.LoadString ("Split Window")
	btnFitToScreen.ToolTip = Docs.LoadString ("Fit Image to Screen")
	btnZoom0.ToolTip = Docs.LoadString ("Zoom 1:1")

	ActionState.BeginGroupUndo "vt"

	ShowSource
	
	For i = 0 To UBound(Params)
		Params(i) = Data.GetDefValue ("\Morphology\Value-" & CStr(i), 2)
	Next

	If Data.GetDefValue ("\Morphology\Reconstruct", 0) Then chkReconstruct.Checked = 1

	Form.InitializePreview
	iFunc = Data.GetDefValue ("\Morphology\Function", 0)
	If iFunc < 0 Then iFunc = 0
	If iFunc > 3 Then iFunc = 3
	Execute "opt" & arrFunc(iFunc) & ".ItemChecked 0"
	Execute "Call opt" & arrFunc(iFunc) & "_OnChangeCheck (0)"
	Execute "opt" & arrFunc(iFunc) & ".Enabled = True"

	Call set_sldZoom

	Data.SetValue "\DockBars\PageInProcessing", True
	ActionManager.HideDockBars
	'прорисовываем форму //
	Application.ProcessMessage 0

	bInit = True

	'Если форма не показывется, не делим окно //
	If Data.GetValue ("\FormManager\DisableFormShow") = 0 Then
		If Data.GetDefValue ("\Morphology\SplitWindow", 0) Then btnSplit.SetPressedState
	End If
	Call btnSplit_Click()
	Call btnFitToScreen_Click

	RunFuncHist(-1)

	'Блокируем команды в контекстном меню //
	StateFit2ImageBeforeForm = ActionState.GetActionState ("Fit2Image")
	StateShowPropertyBeforeForm = ActionState.GetActionState ("ShowPropertyBar")
	StateSendToBeforeForm = ActionState.GetActionState ("SendTo")
	StateCutDocBySelBeforeForm = ActionState.GetActionState ("CutDocBySel")
	
	If StateFit2ImageBeforeForm Then ActionState.SetActionState "Fit2Image", 0
	If StateShowPropertyBeforeForm Then ActionState.SetActionState "ShowPropertyBar", 0
	If StateSendToBeforeForm Then ActionState.SetActionState "SendTo", 0
	If StateCutDocBySelBeforeForm Then ActionState.SetActionState "CutDocBySel", 0

	bSkipEvents = False
End Sub

Sub Form_OnChangePreview()
'здесь ничего нельзя делать из-за рекурсии //
End Sub

Sub Form_OnOK()	
	Form.ApplyPreview	
	ActionManager.SelectMakeVirtual

	ObjectDelete "_tmp_Virt_Image"

	If Not Docs.IsPointerNull(MethodMan) Then MethodMan.StartSetValueLogging

	Data.SetValue "\Morphology\Function", iFunc
	For i = 0 To UBound(Params)
		Data.SetValue "\Morphology\Value-" & CStr(i), Params(i)
	Next
	
	Data.SetValue "\Morphology\Reconstruct", Abs(chkReconstruct.Checked)

	'Запись правильного названия выбранной функции //
	If TypeName (MethodMan) <> "Nothing" Then
		If MethodMan.IsRecording() Then
			sStepName = Eval( "opt" & arrFunc(iFunc) & ".GetItem(0)" )
			MethodMan.RenameCurrentStep sStepName
		End If
	End If

	'Начинаем новую группу для помещения в Undo каждое нажатие Apply //
	ActionState.EndGroupUndo 1
	ActionState.BeginGroupUndo "vt"

	'Не запоминаем деление окна если форма не показывется //
	If Data.GetValue ("\FormManager\DisableFormShow") = 0 Then 
		Data.SetValue "\Morphology\SplitWindow", btnSplit.IsPressed
	End If

	If Not Docs.IsPointerNull(MethodMan) Then MethodMan.FinishSetValueLogging

	Data.SetValue "\Form\WasPressOk", 1
End Sub

Sub Form_OnDestroy()
	ObjectDelete "_tmp_Virt_Image"

	Data.SetValue "\DockBars\PageInProcessing", False
	ActionManager.ShowDockBars

	ActionState.EndGroupUndo 0

	Set f = MainWnd.ActiveFrame
	If TypeName(f) <> "Nothing" Then
		f.GetSplitter.EnableButton -1, -1, 31, 31
	End If
	Set f = Nothing

	'Разблокируем команды в контекстном меню //
	If StateFit2ImageBeforeForm Then ActionState.ResetActionState "Fit2Image"
	If StateShowPropertyBeforeForm Then ActionState.ResetActionState "ShowPropertyBar"
	If StateSendToBeforeForm Then ActionState.ResetActionState "SendTo"
	If StateCutDocBySelBeforeForm Then ActionState.ResetActionState "CutDocBySel"
End Sub

