[Script]
Dim sBrightCaption
Dim StateFit2ImageBeforeForm, StateShowPropertyBeforeForm, StateSendToBeforeForm, StateCutDocBySelBeforeForm 

Const f_Erode = 0
Const f_Dilate = 1
Const f_Open = 2
Const f_Close = 3

Dim arrFunc(5)
arrFunc(f_Erode) = "Erode"
arrFunc(f_Dilate) = "Dilate"
arrFunc(f_Open) = "Open"
arrFunc(f_Close) = "Close"

