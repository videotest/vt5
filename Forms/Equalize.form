Dim strFunc	'active function
Dim FullByFrame, rec 
rec = 0
Const ZoomStep = 0.5
Dim Pane

Dim bSkipEvents			' Флаг для предотвращения повторных действий //
bSkipEvents = True

Dim gl_Script

Sub RunFuncHist(P)
	Select Case P
	    Case 0
			btn1.SetPressedState
			btn2.ResetPressedState
			btn3.ResetPressedState
			btn4.ResetPressedState
			btnAll.ResetPressedState
			Hist.SetPane(0)
	    Case 1
			btn1.ResetPressedState
			btn2.SetPressedState
			btn3.ResetPressedState
			btn4.ResetPressedState
			btnAll.ResetPressedState
			Hist.SetPane(1)
	    Case 2
			btn1.ResetPressedState
			btn2.ResetPressedState
			btn3.SetPressedState
			btn4.ResetPressedState
			btnAll.ResetPressedState
			Hist.SetPane(2)
	    Case 3
			btn1.ResetPressedState
			btn2.ResetPressedState
			btn3.ResetPressedState
			btn4.SetPressedState
			btnAll.ResetPressedState
			Hist.SetPane(3)
	    Case -1
			btnAll.SetPressedState
			btn1.ResetPressedState
			btn2.ResetPressedState
			btn3.ResetPressedState
			btn4.ResetPressedState
			Hist.AddPane(0)
			Hist.AddPane(1)
			Hist.AddPane(2)
	End Select
End Sub

Sub RunFunc()
	'If btnchkFullImage.IsPressed Then 
	If chkFullImage.Checked Then 
		FullByFrame = 1
	Else 
		FullByFrame = 0
	End If

	optEqualize.ItemChecked -1
	optLocalEqualize.ItemChecked -1

	Select Case strFunc
	    Case "Equalize"
			optEqualize.ItemChecked 0
			chkFullImage.Visible = True
			edtMask.Visible = False
			sldMask.Visible = False
			Control3.Visible = False
			Control100.Visible = False
			ActionHelper.Equalize FullByFrame 
			ActionHelper.Equalize FullByFrame 
	    Case "LocalEqualize"
			optLocalEqualize.ItemChecked 0
			chkFullImage.Visible = False
			edtMask.Visible = True
			sldMask.Visible = True
			Control3.Visible = True
			Control100.Visible = True
			ActionHelper.LocalEqualize sldMask.PosLo
			ActionHelper.LocalEqualize sldMask.PosLo
	End Select
End Sub
