Sub CheckSelected
	Dim bChecked
	bChecked = False
	For i = 0 To lstPhases.GetRowCount - 1
		If lstPhases.GetCellAsLogical (1, i) Then
			bChecked = True
			Exit For
		End If
	Next
	btnOk.Disabled = Not bChecked
End Sub

