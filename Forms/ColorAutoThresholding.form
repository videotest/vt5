Sub phases_initialise
	Dim Attaching()

	If bInitFinish Then
		ReDim Attaching(colPh.Count - 1, 1)
		For i = 0 To UBound(Attaching, 1)
			Attaching(i, 0) = colPh.item(i).Color
			Attaching(i, 1) = lstPhases.GetCellAsLogical (1, i)
		Next
	Else
		ReDim Attaching(edtPhasesN.ValueInt - 1, 1)
		For i = 0 To Ubound(Attaching, 1)
			Attaching(i, 0) = GetFormSettings ( "ColorAttaching\Color" & CStr(i), "(0,0,0)" )
			Attaching(i, 1) = GetFormSettings ( "ColorAttaching\Phase" & CStr(i), 0 )
		Next
	End If

	For i = 0 To lstPhases.GetRowCount - 1
		lstPhases.DeleteRow 0
	Next

	colPh.RemoveAll
	n = edtPhasesN.ValueInt
	bHasCheck = 0
	For i = 0 To n - 1
		iActivePhase = i
		lstPhases.AddRow i
		colPh.Add i, ""
		Set colPh.item(i) = New ColorPhase
		colPh.item(i).Name = CStr(i + 1)
		colPh.item(i).Color = Data.GetDefValue("\SegmColorPhases\Color" & CStr(i), "(0,0,0)")
		colPh.item(i).Weight = 0

		iFoundColor = FindNearColor( Attaching, colPh.item(i).Color )
		If iFoundColor > -1 Then
			bCheck = Attaching( iFoundColor, 1 )
			lstPhases.SetCellAsLogical 1, i, bCheck
			bHasCheck = bHasCheck Or bCheck
		Else
			lstPhases.SetCellAsLogical 1, i, 0
		End If
	Next

	'         //
	'       //
	If Data.GetValue ("\FormManager\DisableFormShow") And bHasCheck = 0 Then
		MainWnd.ShowHiddenForm
		Call Form_OnShow()	'        -   //
	End If

	Data.SetValue "\Phase\PhasesCount", colPh.Count

	Call CheckSelected
End Sub


