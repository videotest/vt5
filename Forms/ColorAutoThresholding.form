		If bInitFinish Then
			If optMode.ItemChecked = 2 Then SwitchPhase iActivePhase
			ActionHelper.SegmColorPhases
			ActionHelper.SegmColorPhases
		End If
	End If
	Set frame = Nothing
	
	bSplitInProcessing = False
End Sub



