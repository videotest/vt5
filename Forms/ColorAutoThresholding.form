'optMode event handlers.
'***************************************************************
Sub optMode_OnChangeCheck(lItem)
	Dim ph()
'	ActionState.TerminateCurrentAction

	Data.SetValue "\Phase\PhasesCount", colPh.Count

	Select Case lItem
	  Case 0

		SwitchPhase -1
		Call ShowPhases(0)

	  Case 1

		SwitchPhase -1
		Call ShowPhases(1)

	  Case 2
		
		Data.SetValue "\Phase\PhasesCount", 0
		SwitchPhase iActivePhase
		Call ShowPhases(0)
		Call lstPhases_CurentRowChange(lstPhases.GetCurentRow)

	End Select
End Sub

'===============================================================
'     (1) /  (0) //
'===============================================================
Sub ShowPhases(bShow)
	Dim ph()

	If bShow Then
		iSelectedCount = 0
		For i = 0 To colPh.Count - 1
			If lstPhases.GetCellAsLogical (1, i) Then
				Data.SetValue "\SegmColorPhases\Phase" & CStr(i), 255	'128 + colPh.Count
				iSelectedCount = iSelectedCount + 1
			Else
				Data.SetValue "\SegmColorPhases\Phase" & CStr(i), 0
			End If
		Next
		'If iSelectedCount = 0 Then
		'	iRow = lstPhases.GetCurentRow
		'	lstPhases.SetCellAsLogical 1, iRow, 1
		'	Data.SetValue "\SegmColorPhases\Phase" & CStr(iRow), 128 + colPh.Count
		'End If
	Else
		For i = 0 To colPh.Count - 1
			Data.SetValue "\SegmColorPhases\Phase" & CStr(i), 128 + i
		Next
	End If

	If Not bFormDestroy And bShowPhasesMode <> bShow Then
		If bShowPhasesMode <> -3 Then Form.CancelPreview
		bShowPhasesMode = bShow
		ActionHelper.SegmColorPhases
	End If
End Sub


