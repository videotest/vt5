	If nColumn = 1 Then
		'iSelectedCount = 0
		'For i = 0 To colPh.Count - 1
		'	If lstPhases.GetCellAsLogical (1, i) Then iSelectedCount = iSelectedCount + 1
		'Next
		'If iSelectedCount = 0 Then optMode.ItemChecked = 0
