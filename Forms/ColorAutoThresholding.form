Class ColorPhase
	Dim mvarName
	Dim mvarColor
	Dim mvarWeight

	Public Property Let Name(vData)
		mvarName = vData
		lstPhases.SetCellValue 0, iActivePhase, CStr(vData)
	End Property

	Public Property Get Name()
		Name = mvarName
	End Property

	
	Public Property Let Color(vData)
		mvarColor = vData
		'lstPhases.SetCellValueColorRGB 2, iActivePhase, CStr(vData)
		OLEColor = gl_Script.ConvertColorStrToBin( vData )
		lstPhases.SetCellBackColor 2, iActivePhase, OLEColor
		lstPhases.SetCellValue 2, iActivePhase, ""
		Data.SetValue "\Phase\Phase" & CStr(iActivePhase + 1) & "Color", vData
	End Property

	Public Property Get Color()
		Color = mvarColor
	End Property

	Public Property Let Weight(vData)
		mvarWeight = vData
		lstPhases.SetCellValue 3, iActivePhase, vData
		Data.SetValue "\SegmColorPhases\Weight" & CStr(iActivePhase), Exp(vData / 50)
	End Property

	Public Property Get Weight()
		Weight = mvarWeight
	End Property
End Class


