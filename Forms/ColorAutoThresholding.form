'==============================================================
'       (r,g,b) //
' arrColors -     //
' sGivenColor -   //
'      //
' FindNearColor = -1     -  //
'==============================================================
Function FindNearColor(arrColors, sGivenColor)
	FindNearColor = -1
	sGivenColor = Replace(sGivenColor, " ", "")
	sGivenColor = Replace(sGivenColor, "(", "")
	sGivenColor = Replace(sGivenColor, ")", "")
	arrPanes1 = Split(sGivenColor, ",")
	iPanesCount = UBound(arrPanes1)
	best = 0
	r_best = 1e20
	For i = 0 To UBound(arrColors, 1)
		sColor = Replace(arrColors(i, 0), " ", "")
		sColor = Replace(sColor, "(", "")
		sColor = Replace(sColor, ")", "")
		arrPanes2 = Split(sColor, ",")
		r = 0
		If UBound(arrPanes2) <> iPanesCount Then Exit Function
		For nPane = 0 To iPanesCount
'			r = r + ( CLng( arrPanes2(nPane) ) - CLng( arrPanes1(nPane) ) ) ^ 2
			r = r + ( CLng( arrPanes2(nPane) ) - CLng( arrPanes1(nPane) ) ) * ( CLng( arrPanes2(nPane) ) - CLng( arrPanes1(nPane) ) )
		Next

		'    //
		p = 0
		r1 = 0
		r2 = 0
		For nPane = 0 To iPanesCount
			r1 = r1 + CLng( arrPanes1(nPane) ) ^ 2
			r2 = r2 + CLng( arrPanes2(nPane) ) ^ 2
			p = p + CLng( arrPanes1(nPane) ) * CLng( arrPanes2(nPane) )
		Next
		delta = r1 - p * p / ( r2 + 1e-20 )

		r = r * 0.1 + delta * 0.9
		'end //

		If r < r_best Then
			best = i
			r_best = r
		End If
	Next
	FindNearColor = best
End Function



