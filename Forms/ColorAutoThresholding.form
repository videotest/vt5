Dim Flag1, Flag2
Flag1 = false
Flag2 = false

'   -  //
Sub edtPhasesN_DataChange()
	If Flag1 Then
		Flag2 = true
		Exit Sub
	End If

	Flag1 = true
	Do
		Flag2 = false
		Call ChangePhasesN
	Loop While Flag2
	Flag1 = false
End Sub

sub ChangePhasesN()
	dim n, n1
	n = edtPhasesN.ValueInt
	FindPhasesColor n, 0
	Form.TerminateCurrentAction
'	gl_Script.SaveLog "cat.log", "FindPhasesColor " & CStr(n)
	If Flag2 Then Exit Sub

	Call phases_initialise

	If Not bInitFinish Then Exit Sub

	bShowPhasesMode = -2
	Call optMode_OnChangeCheck(optMode.ItemChecked)
	Call lstPhases_CurentRowChange(0)

'	n1 = edtPhasesN.ValueInt
'	gl_Script.SaveLog "cat.log", "FindPhasesColor " & CStr(n) & "->" & CStr(n1)
End Sub


