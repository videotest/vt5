[Script]
Dim intAccuracy

'***************************************************************
'Form event handlers.
'***************************************************************
Sub Form_OnInitialize()

	lstMarkerFont.UseSimpleFontType = 1
	lstMarkerFont.SetColumnCaption 0, Docs.LoadString ("Font")
	lstMarkerFont.SetColumnCaption 1, Docs.LoadString ("Marker Color")
	lstMarkerFont.SetColumnCaption 2, Docs.LoadString ("Text Color")
	fldAccuracyTitle.Text = Docs.LoadString ("Decimal places:")

	Data.SetValue "\tmp\Font\Name", Data.GetValue("\EditMarker\TextFont_Name")
	Data.SetValue "\tmp\Font\Size", Data.GetValue("\EditMarker\TextFont_Height")
	Data.SetValue "\tmp\Font\Bold", Data.GetValue("\EditMarker\TextFont_Bold")
	Data.SetValue "\tmp\Font\Italic", Data.GetValue("\EditMarker\TextFont_Italic")
	lstMarkerFont.RestoreFromData

	intAccuracy = Data.GetDefValue("\EditMarker\Accuracy", 2)
	'fldAccuracy.Text = UserNumFormat(intAccuracy)
	edtAccuracy.ValueInt = intAccuracy
End Sub

Sub Form_OnOK()
	lstMarkerFont.StoreToData
	Data.SetValue "\EditMarker\TextFont_Name", Data.GetValue("\tmp\Font\Name")
	Data.SetValue "\EditMarker\TextFont_Height", Data.GetValue("\tmp\Font\Size")
	Data.SetValue "\EditMarker\TextFont_Bold", Data.GetValue("\tmp\Font\Bold")
	Data.SetValue "\EditMarker\TextFont_Italic", Data.GetValue("\tmp\Font\Italic")

	Form.UpdateSystemSettings

	Data.SetValue "\EditMarker\Accuracy", intAccuracy
End Sub

Sub Form_OnCancel()
End Sub

Sub Form_OnDestroy()
	Data.RemoveValue("\tmp")
End Sub


'***************************************************************
'btnAccuracyLess event handlers.
'***************************************************************
Sub btnAccuracyLess_Click()
	intAccuracy = intAccuracy - 1
	If intAccuracy < 0 Then intAccuracy = 0
	fldAccuracy.Text = UserNumFormat(intAccuracy)
End Sub

'***************************************************************
'btnAccuracyMore event handlers.
'***************************************************************
Sub btnAccuracyMore_Click()
	intAccuracy = intAccuracy + 1
	If intAccuracy > 6 Then intAccuracy = 6
	fldAccuracy.Text = UserNumFormat(intAccuracy)
End Sub

'***************************************************************
' Преобразование формата в 0.000 //
'***************************************************************
Function UserNumFormat(intFraction)
	UserNumFormat = " 1"
	If intFraction > 0 Then
		UserNumFormat = " 1." & String(intFraction, "0")
	End If
End Function

'***************************************************************
'edtAccuracy event handlers.
'***************************************************************
sub edtAccuracy_DataChange()
	intAccuracy = edtAccuracy.ValueInt
end sub
