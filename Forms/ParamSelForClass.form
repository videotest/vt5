[Script]
id = "ParamSelForClass"

Dim global, rec , strClassFile
Dim AllParamsArray			' 0- имя 1- key 2- On/Off
Dim ParamsClassifierArray, ParamsClassifierArrayTMP
Dim MeasuredParamsArray, CountMeasuredParams
Dim MarkMeasColor, MarkClassColor, UnMarkColor
Dim nd
MarkMeasColor = 9568200
MarkClassColor = 65535	'10551295
UnMarkColor = 16777215
Set global = Application.GetGlobalScript("")
'***************************************************************
'Form event handlers.
'***************************************************************
Sub Form_OnInitialize()
	rec = 1
	If not global.isDocImage then
		Set nd = Data
	Else
		If global.GetNameStatTable() <> "" Then 
			Set nd = global.GetTableNdata()
		ElseIf global.GetNameObjectList() <> "" Then
			Set ObjList = Objects.GetObjectByName (global.GetNameObjectList())
			Set nd = ObjList.GetPrivateNamedData
			Set ObjList = Nothing
		End If
	End If	
	
	lstGroups.SetColumnCaption 0, Docs.LoadString ("Group")
	lstGroups.SetColumnCaption 1, ""
	lstGroups.SetColumnCaption 2, ""
	lstMeasParam.SetColumnCaption 0, Docs.LoadString ("Parameter")
	lstMeasParam.SetColumnCaption 1, Docs.LoadString ("Result")
	
	strClassFile = Data.Getvalue ("\Classes\ClassFile")
	StrFromINI = Application.GetPrivateProfileString(strClassFile,"KeysInfo","KeysEnum","0")	
	ParamsClassifierArray = Split( StrFromINI, ",")
	FillGroupsParam()
	CheckMeasuredParams()

	Data.SetSection "\measurement\parameters"
	n = Data.GetEntriesCount 
	Redim AllParamsArray (n+1,2)
	For i=0 to n-1
		ParamName = Data.GetEntryName(i)
		If ParamName <> "" Then		
		name = Data.Getvalue ("\measurement\parameters\"+ParamName+"\UserName" )
		AllParamsArray(i,0) = name
			AllParamsArray(i,1) = global.GetParamKey(ParamName)			
		AllParamsArray(i,2) = 0
		For j=0 to UBound(ParamsClassifierArray)
			If CLng(ParamsClassifierArray(j)) = AllParamsArray(i,1) Then
				AllParamsArray(i,2) = 1
			End If
		Next
		End If
	Next
	
	For i=0 to lstGroups.GetRowCount()-1
		lstGroups_CurentRowChange i 
	Next
	lstGroups_CurentRowChange 0
	edtClass.Text = ""
	edtMeas.Text = ""
	edtClass.EditBackColor MarkClassColor
	edtMeas.EditBackColor MarkMeasColor
	lstMeasParam.UseFullRowSelect 1
	rec = 0
End Sub

Sub Form_OnOK()
	nClasses = cint(Application.GetPrivateProfileString(strClassFile,"Classes","ClassCount","0"))
	sKeysInfo = ""
	strParams = ""
	For i=0 to UBound(AllParamsArray)
		If AllParamsArray(i,2) = 1 Then
			strParams = cstr(AllParamsArray(i,1))
			If len(sKeysInfo)>0 Then sKeysInfo = sKeysInfo + ","
			sKeysInfo = sKeysInfo + strParams
			For k=0 to nClasses
				str = Application.GetPrivateProfileString(strClassFile,"KeyData:"+strParams,cstr(k),"0")
				If Instr(str,",") Then
					arPar = Split(str,",")
					Application.WritePrivateProfileString strClassFile,"KeyData:"+strParams,cstr(k), cstr(arPar(0))+","+cstr(arPar(1))
				else
					Application.WritePrivateProfileString strClassFile,"KeyData:"+strParams,cstr(k), "0,0"
				End If	
			Next
			Data.SetValue "\measurement\parameters\" + global.GetParamMainName(strParams) + "\Enable", 1
			Application.WritePrivateProfileString strClassFile,"ParamInfo",strParams, global.GetParamUserName(strParams)				
		End If 
	Next
	If strParams = "" Then
		MsgBox Docs.LoadString("All parameters are disabled (switched off), changes to classifier are not made!"),vbOKOnly,Docs.LoadString("Attention!") 
	else 
		Application.WritePrivateProfileString strClassFile,"KeysInfo","KeysEnum", sKeysInfo
	End If
End Sub

Sub Form_OnDestroy()
	Set nd = Nothing
	Set global  = Nothing
End Sub


Sub FillGroupsParam()
	rec = 1
	For i=lstGroups.GetRowCount() to  0 step -1
		lstGroups.DeleteRow i 
	Next
	tmp = Data.GetSection() 
	Data.SetSection "\measurement\groups"
	grCount = Data.GetEntriesCount 
	n = 0
	For i=0 To grCount-1
		grName = Data.GetEntryName(i)
		If grName <> "Extend" Then
			lstGroups.AddRow(-1)
			lstGroups.SetCellValue 0, n, grName
			n = n + 1
		End If
	Next
	rec = 0
	Data.SetSection tmp
End Sub

'***************************************************************
'lstMeasParam event handlers.
'***************************************************************
sub lstMeasParam_CurentRowChange(nRow)
	clr = lstMeasParam.GetCellBackColor (0,nRow)
	lstMeasParam.ActiveCellBackColor clr
	lstMeasParam.ActiveCellTextColor 0
end sub

sub lstMeasParam_DataChange(nColumn, nRow)
	strParams = ""
	For i=0 to UBound(AllParamsArray)
		If lstMeasParam.GetCellAsString(0,nRow) = AllParamsArray(i,0) Then
			If lstMeasParam.GetCellAsLogical(1,nRow) Then
				AllParamsArray(i,2) = 1
			Else
				AllParamsArray(i,2) = 0
			End If
			exit for
		End If 
	Next
	lstGroups_CurentRowChange(lstGroups.GetCurentRow())
End Sub

'***************************************************************
'lstGroups event handlers.
'***************************************************************
sub lstGroups_CurentColumnChange(nColumn)
	If nColumn > 0 Then
		lstGroups.SetCurentCol 0
	End If
end sub
sub lstGroups_CurentRowChange(nRow)
	If rec Then exit sub	
	For i=lstMeasParam.GetRowCount() to  0 step -1
		lstMeasParam.DeleteRow i 
	Next	
	tmp = Data.GetSection() 
	n = 0
	GrName = lstGroups.GetCellAsString(0,nRow)
	Data.SetSection "\measurement\groups\" + GrName
	pCount = Data.GetEntriesCount 
	lstGroups.SetCellBackColor 1, nRow, UnMarkColor
	lstGroups.SetCellBackColor 2, nRow, UnMarkColor
	For i=0 To pCount-1
		ParamName = Data.GetEntryName(i)
		name = Data.Getvalue ("\measurement\parameters\"+ParamName+"\UserName" )
		key = global.GetParamKey(ParamName)
		lstMeasParam.AddRow(-1)
		lstMeasParam.SetCellValue 0, i, name
		If ParamInClass(key) Then
			lstMeasParam.SetCellAsLogical 1, i, 1
			lstGroups.SetCellBackColor 2, nRow, MarkClassColor
		End If
		If CheckParamMeasured(name) Then
			lstMeasParam.SetCellBackColor 0, i, MarkMeasColor
			lstMeasParam.SetCellBackColor 1, i, MarkMeasColor
			lstGroups.SetCellBackColor 1, nRow, MarkMeasColor
		End If		
	Next
	Data.SetSection tmp
	lstMeasParam_CurentRowChange 0
end sub

Function ParamInClass(key)
	ParamInClass = 0
	For i=0 To UBound(AllParamsArray) 
		If AllParamsArray(i,1) = CLng(key) And AllParamsArray(i,2) = 1 Then 
			ParamInClass = 1
			exit Function 
		End If
	next
End Function

Sub CheckMeasuredParams()
	Set objStatTable = global.GetStatTable()
	If not Docs.IsPointerNull(objStatTable)  Then	
		pos = objStatTable.getFirstParamPos()
		N = 0
		While pos <> 0
			key = objStatTable.GetParamKey(pos)
			If key >=0  Then				
				N = N + 1 
			End If
			pos = objStatTable.getNextParamPos(pos)			 
		Wend
		CountMeasuredParams = N
		redim MeasuredParamsArray (N)
		pos = objStatTable.getFirstParamPos()

		i = 0
		While pos <> 0
			key = objStatTable.GetParamKey(pos)
			If key >=0  Then	
				MeasuredParamsArray(i) = global.GetParamUserName(key)
				i = i + 1 
			End If
			pos = objStatTable.getNextParamPos(pos)			 
		Wend
	End If
	Set objStatTable = Nothing
End Sub
		
Function CheckParamMeasured(name)
	CheckParamMeasured = 0
	If CountMeasuredParams = 0 Then Exit Function
	If UBound(MeasuredParamsArray) >= 0 Then	
		For i=0 To UBound(MeasuredParamsArray)-1 
			If MeasuredParamsArray(i) = name Then 
				CheckParamMeasured = 1
				exit Function 
			End If
		next	
	End If
End Function