sub btnDelete_Click()
	str = ulbList.GetParamSection ( ulbList.ActiveItem )
	name = Data.Getvalue (str+"\UserName")
	str1 = (Docs.LoadString ("Parameter") + " """ + name + """ " + Docs.LoadString ("will be deleted!"))
	Answer = MsgBox (str1 + vbLf + Docs.LoadString ("Are you sure?"), vbYesNo+vbDefaultButton2,Docs.LoadString ("Delete Parameter"))
	If Answer = vbYes Then
		Data.RemoveValue str
		str = "\measurement\groups\"+Docs.LoadString("User Defined")+ "\"+name
		Data.RemoveValue str
		ulbList.Refresh()
		ulbList.ActiveItem = ulbList.GetItemCount()-1
		ulbList_OnActiveItemChange ulbList.GetItemCount()-1
	End If
	MainWnd.UpdateSystemSettings
	CheckEnable()
	LoadMeasParams()
end sub

