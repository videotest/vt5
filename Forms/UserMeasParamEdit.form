'***************************************************************
sub btnNew_Click()
	Data.Setvalue "\StatCfg\tmp",1
	FormManager.ExecuteModal "NewParam"
	NewName = Data.Getvalue ("\StatCfg\tmp")
	Graphics = Data.Getvalue ("\StatCfg\tmp1")
	If NewName = "" Then Exit Sub
	If MeasParamExist(NewName,Graphics) Then Exit Sub
	NewKey = NewID()
	Data.Setvalue sMP+ NewName + "\Key", CStr(NewKey)
	Data.Setvalue sMP+ NewName + "\Enable",1
	Data.Setvalue sMP+ NewName + "\UserName", NewName
	Data.Setvalue sMP+ NewName + "\Graphics", Graphics
	Data.Setvalue sMP+ NewName + "\Format","%0.3f"
	Data.Setvalue sMP+ NewName + "\ExprString","{"+CStr( NewKey )+"}="
	Data.Setvalue sMP+ NewName + "\GreekSymbol",-1
	dupDispl.Expression = "{"+CStr( NewKey )+"}="
	dupDispl.Draw()
	iNew = 1
	drw()
	ulbList.Refresh()
	ulbList.ActiveItem = ulbList.GetItemCount()-1
	iEditLine = ulbList.ActiveItem
	LoadMeasParams()
end sub

sub LoadMeasParams()
	tmp = Data.GetSection()
	cbbMeasParams.DeleteContents
	cbbMeasParSymbol.DeleteContents
	Data.SetSection sMP
	ItemsCount = Data.GetEntriesCount
	n = 0
	For i=ItemsCount-1 to 0 step -1
		Data.SetSection sMP
		ID = Data.GetEntryName(i)
		If ID <> "" Then		
			key = Data.Getvalue (sMP + ID + "\Key")
			If key = START_ID-1 or key = "" Then
				Data.RemoveValue sMP + ID 			
				ItemsCount = ItemsCount - 1
			End If
		End If
	next
	n=0
	Data.SetSection sMG
	ItemsCount = Data.GetEntriesCount
	For i=0 to ItemsCount -1 
		Data.SetSection sMG
		ID = Data.GetEntryName(i)
		If ID <> "Extend" Then		
			Data.SetSection sMG + ID
			ItemsCountInside = Data.GetEntriesCount
			For j=0 to ItemsCountInside-1
				par = Data.GetEntryName(j)
				UserName = Data.Getvalue (sMP + par+"\UserName")
				key = Data.Getvalue (sMP + par+"\Key")
				cbbMeasParams.InsertString n, UserName				
				cbbMeasParSymbol.InsertString n, global.GetParamGraphicsName(key)
				n = n + 1
			Next	
		End If
	Next
	
	cbbMeasParams.SetSel 0
	cbbMeasParSymbol.SetSel 0
	cbbMeasParams_OnSelectChange()
	ulbList.Refresh() 
	Data.SetSection tmp
	ReCreateUserParam()
end sub

