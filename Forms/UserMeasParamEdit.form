		iEdit = 1
		key = global.GetParamKey (cbbMeasParams.GetString(cbbMeasParams.GetSel))
		MainName = global.GetParamMainName(key)	
		Data.Setvalue sMP + MainName + "\Graphics",cbbMeasParSymbol.GetText()
		ulbList.Refresh()
		dupDispl.Expression = Data.Getvalue (str+"\ExprString")		
		dupDispl.Draw()
		cbbMeasParSymbol.ReplaceString n, cbbMeasParSymbol.GetText()
