'***************************************************************
'btnErase event handlers.
'***************************************************************
sub btnErase_Click()
	s = dupDispl.Expression
	l = len(s)
	a = mid(s,l)

	If a = "=" Then
		BeginEdit = 1
		CheckEnable()
		exit sub
	End If
	For i=0 to UBound(sFuncs)
		If l - InStrRev(s,sFuncs(i)) < 4 Then
			s = left(s,l-Len(sFuncs(i))-1)
			If s = "" Then s = " "
			dupDispl.Expression = s
		End If
		GoOut = 1
	Next
	If not GoOut Then
		If a <> "}" Then
			s = left(s,l-1)
			If s = "" Then s = " "
			dupDispl.Expression = s
		else
			l = InStrRev(s, "{")
			s = left(s,l-1)
			If s = "" Then s = " "
			dupDispl.Expression = s
		End If
	End If
	l = len(s)
	a = mid(s,l)
	If a = "=" Then BeginEdit = 1
	iAddEnable = 1
	CheckEnable()
end sub

