Function NewID()
	tmp = Data.GetSection()
	Data.SetSection sMP
	iTotal = Data.GetEntriesCount
	NewID = START_ID - 1
	already = 0
	do	
		NewID = NewID + 1
		already = 0
		For i=0 To iTotal-1
			pName = Data.GetEntryName(i)
			key = Data.Getvalue (sMP + pName + "\Key")
			If cstr(key) = cstr(NewID) Then 
				already =1
				Exit for
			End If
		Next
	Loop while already = 1
	Data.SetSection tmp
End Function

Function MeasParamExist(un,gr)
	Data.SetSection sMP
	pCount = Data.GetEntriesCount
	For i=0 To pCount-1
		id = Data.GetEntryName(i)
		idgr = Data.Getvalue (sMP + id + "\Graphics")
		If id = un Then
			If idgr = "" Then
				Data.RemoveValue sMP + id
			else
			MeasParamExist = true
			MsgBox Docs.LoadString ("This parameter already exists!"), vbOkOnly, Docs.LoadString ("Error!")
			Exit Function
			End If
		End If
		If idgr = gr Then
			MeasParamExist = true
			MsgBox Docs.LoadString ("This parameter symbol already exists!"), vbOkOnly, Docs.LoadString ("Error!")
			Exit Function
		End If
	next
	MeasParamExist = false
End Function

Sub ReCreateUserParam()
	tmp = Data.GetSection()
	Data.RemoveValue "\measurement\groups\"+Docs.LoadString("User Defined")
	MainWnd.UpdateSystemSettings
	Data.SetSection sMP
	ItemsCount = Data.GetEntriesCount
	For i=0 to ItemsCount-1
		Data.SetSection sMP
		ID = Data.GetEntryName(i)
		key = Data.Getvalue (sMP + ID + "\Key")
		If key>=START_ID Then		
		str = Data.Getvalue (sMP + ID + "\ExprString")
		If CDbl(key)>=START_ID and CDbl(key)<196609 And InStr(str,"#")=0 Then
			Data.Setvalue "\measurement\groups\" + Docs.LoadString("User Defined") + "\" + ID, 1
			End If
		End If
	Next		
	Data.SetSection tmp
End Sub

