sub btnAdd_Click()
	str = dupDispl.GetLastError
	If str <> Docs.LoadString("No errors") Then
		MsgBox str, vbOkOnly, Docs.LoadString ("Error!")
		exit sub
	End If

'	If NewName <> "" Then
'		For i=0 to ulbList.GetItemCount-1
'			str = ulbList.GetParamSection(i)
'			gr = Data.Getvalue (str + "\UserName")
'			If gr = NewName Then
'				iEditLine = i
'				exit for
'			End If
'		Next
'	End If
	If iEditLine >= 0 Then
		str = ulbList.GetParamSection(iEditLine)
		dupDispl.Draw()
		Data.Setvalue str + "\ExprString", dupDispl.Expression
		If iEdit = 1 Then 
			dupDispl.Draw()
			dupDispl.Expression = " "
			dupDispl.Draw()
			dupDispl.Expression = Data.Getvalue (str+"\ExprString")
			iEdit = 0
		End If		
		ulbList.Refresh()
		iAddEnable = 0
		CheckEnable()
		ReCreateUserParam()
		LoadMeasParams()
		exit sub
	else
		str = sMP + NewName 
		Data.Setvalue sMP+ NewName + "\Key", CStr(NewKey)
		Data.Setvalue sMP+ NewName + "\Enable",1
		Data.Setvalue sMP+ NewName + "\UserName", NewName
		Data.Setvalue sMP+ NewName + "\ExprString","{"+CStr( NewKey )+"}="
		Data.Setvalue sMP+ NewName + "\GreekSymbol",-1
		dupDispl.Expression = "{"+CStr( NewKey )+"}="
	End If
	ulbList.Refresh()
	dupDispl.Draw()
	ulbList.ActiveItem = ulbList.GetItemCount()-1
	ulbList_OnActiveItemChange(ulbList.GetItemCount()-1)
	iAddEnable = 0
	CheckEnable()
	LoadMeasParams()
end sub

