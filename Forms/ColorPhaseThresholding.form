Sub set_range_color(nRange)
	phN = find_phase( lstPhases.GetCellAsString(2, nRange) )
	If phN = -1 Then
		sColor = "(255,255,255)"
	Else
		sColor = lstGlPhases.GetCellAsColorRGB( 1, phN )
	End If
	lstPhases.SetCellValueColorRGB 1, nRange, CStr(sColor)
End Sub

