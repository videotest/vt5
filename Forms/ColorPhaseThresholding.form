dim Flag1, Flag2
Flag1 = false
Flag2 = false

'   -  //
sub edtPhasesN_DataChange()
	if Flag1 then
		Flag2 = true
		exit sub
	end if

	Flag1 = true
	do
		Flag2 = false
		Call ChangePhasesN
	loop while Flag2
	Flag1 = false
	bClassifierModified = True
end sub

sub ChangePhasesN()
	FindPhasesColor edtPhasesN.ValueInt, 0

	Call phases_initialise

	If Not bInitFinish Then Exit Sub

	bShowPhasesMode = -3
	Call optMode_OnChangeCheck(optMode.ItemChecked)
	Call lstPhases_CurentRowChange(0)
end sub



