'==============================================================
Sub create_phases_classifier(sClassifier)
	Dim n, i, sValue, strClassFile
	Dim AssignedColors()

	strClassFile = Data.GetValue("\Paths\Classes") & sClassifier & ".class"
	n = lstGlPhases.GetRowCount
	Application.WritePrivateProfileString strClassFile, "Classes", "ClassCount", n
	For i = 0 To n - 1
		sValue = lstGlPhases.GetCellAsColorRGB(1, i) & "," & _
		  lstGlPhases.GetCellAsString(0, i)
		Application.WritePrivateProfileString strClassFile, "Classes", CStr(i), sValue
	Next
	Data.SetValue "\Classes\ClassFile", strClassFile

	Application.WritePrivateProfileString strClassFile, "Classes", "ColorsCount", edtPhasesN.ValueInt

	ReDim AssignedColors(n)
	For i = 0 To colPh.Count - 1
		f = find_phase( lstPhases.GetCellAsString (2, i) ) + 1
		If AssignedColors(f) = "" Then
			AssignedColors(f) = colPh.Item(i).Color
		Else
			AssignedColors(f) = AssignedColors(f) & "+" & colPh.Item(i).Color
		End If
	Next

	For i = 0 To n
		Application.WritePrivateProfileString strClassFile, "AssignedColors", CStr(i-1), AssignedColors(i)
	Next
End Sub

