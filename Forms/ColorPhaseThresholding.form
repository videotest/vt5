sub lstGlPhases_DataChange(nColumn, nRow)
	If nColumn = 0 Then
		If ChangePhaseName(nRow) = 0 Then lstGlPhases.SetCellValue 0, nRow, sOldPhaseName
	Else
		Data.SetValue "\Phase\Phase" & CStr(nRow + 1) & "Color", lstGlPhases.GetCellAsColorRGB (1, nRow)
		If optMode.ItemChecked = 1 Then Call optMode_OnChangeCheck(1)
		bClassifierModified = True
		
		For i = 0 To colPh.Count - 1
			Call set_range_color(i)
		Next
	End If
end sub

sub lstGlPhases_CurentRowChange(nRow)
	sOldPhaseName = lstGlPhases.GetCellAsString (0, nRow)
end sub

'==============================================================
'    //
'==============================================================
Function ChangePhaseName(nRow)
	Dim ph()
	ChangePhaseName = 0
	n = lstGlPhases.GetRowCount

	strNewPhaseName = lstGlPhases.GetCellAsString (0, nRow)
	If Trim(strNewPhaseName) = "" Then Exit Function

	bExists = False
	For i = 0 To n - 1
		If i <> nRow And strNewPhaseName = lstGlPhases.GetCellAsString(0, i) Then
			bExists = True
			Exit For
		End If
	Next
	If bExists Then
		MsgBox sPhaseExists, vbExclamation, gl_script.sVT
		Exit Function
	End If

	ReDim ph(n-1)
	For i = 0 To n - 1
		ph(i) = lstGlPhases.GetCellAsString(0, i)
	Next

	For i = 0 To lstPhases.GetRowCount - 1
		lstPhases.ClearCombo 2, i
		lstPhases.AddComboString 2, i, sIgnoreRange
		For j = 0 To n - 1
			lstPhases.AddComboString 2, i, ph(j)
		Next
		If lstPhases.GetCellAsString (2, i) = sOldPhaseName Then lstPhases.SetCellValue 2, i, strNewPhaseName
	Next

	sOldPhaseName = strNewPhaseName
	ChangePhaseName = 1
	
	bClassifierModified = True
End Function

'***************************************************************
'btnAddPhase event handlers.
'***************************************************************
sub btnAddPhase_Click()
	n = lstGlPhases.GetRowCount
	strNewPhaseName = Trim(InputBox(sInputPhaseName, gl_script.sVT, sPhaseText & CStr(n+1)))
	If strNewPhaseName = "" Then Exit Sub
	strNewPhaseName = Left(strNewPhaseName, 20)
	
	bExists = False
	For i = 0 To n - 1
		If strNewPhaseName = lstGlPhases.GetCellAsString(0, i) Then
			bExists = True
			Exit For
		End If
	Next
	If bExists Then
		MsgBox sPhaseExists, vbExclamation, gl_script.sVT
		Exit Sub
	End If

	lstGlPhases.AddRow -1

	lstGlPhases.SetCellValue 0, n, strNewPhaseName
	sColor = "(" & CStr(Rnd * 255) & "," & CStr(Rnd * 255) & "," & CStr(Rnd * 255) & ")"
	lstGlPhases.SetCellValueColorRGB 1, n, sColor
	Call lstGlPhases_DataChange(1, n)

	lstGlPhases.SetCurentRow lstGlPhases.GetRowCount - 1

	For i = 0 To lstPhases.GetRowCount - 1
		lstPhases.AddComboString 2, i, strNewPhaseName
	Next
	btnRemovePhase.Disabled = ( lstGlPhases.GetRowCount < 3 )

	'      //
	nRow = lstPhases.GetCurentRow
	lstPhases.SetCellValue 2, nRow, strNewPhaseName
	Call lstPhases_DataChange(2, nRow)

	bClassifierModified = True
end sub

'***************************************************************
'btnRemovePhase event handlers.
'***************************************************************
sub btnRemovePhase_Click()
	'nRowGL = lstGlPhases.GetCurentRow
	'sPhase = lstGlPhases.GetCellAsString (0, nRowGL)

	nRow = lstPhases.GetCurentRow
	sPhase = lstPhases.GetCellAsString (2, nRow)
	nRowGL = find_phase(sPhase)
	If nRowGL < 0 Then Exit Sub
	
	If MsgBox(sDeletePhase & " """ & sPhase & """?", vbQuestion + vbYesNo, gl_Script.sVT) = vbYes Then

		bWasRemove = False
		For i = 0 To lstPhases.GetRowCount - 1
			lstPhases.RemoveComboItem 2, i, nRowGL + 1
			If lstPhases.GetCellAsString (2, i) = sPhase Then
				lstPhases.SetCellValue 2, i, sIgnoreRange
				bWasRemove = True
			End If
			Call set_range_color(i)
		Next
		lstGlPhases.DeleteRow nRowGL
		
		For i = nRowGL To lstGlPhases.GetRowCount - 1
			Call lstGlPhases_DataChange(1, i)
		Next

		If bWasRemove And optMode.ItemChecked > 0 Then
			bShowPhasesMode = -2
			Call optMode_OnChangeCheck(optMode.ItemChecked)
		End If

		bClassifierModified = True

	End If

	btnRemovePhase.Disabled = ( lstGlPhases.GetRowCount < 3 )
end sub

'==============================================================
'       (r,g,b) //
' arrColors -     //
' sGivenColor -   //
'      //
' FindNearColor = -1     -  //
'==============================================================
Function FindNearColor(arrColors, sGivenColor)
	FindNearColor = -1
	If UBound(arrColors, 2) < 0 Then Exit Function

	sGivenColor = Replace(sGivenColor, " ", "")
	sGivenColor = Replace(sGivenColor, "(", "")
	sGivenColor = Replace(sGivenColor, ")", "")
	arrPanes1 = Split(sGivenColor, ",")
	iPanesCount = UBound(arrPanes1)
	best = 0
	r_best = 1e20
	For i = 0 To UBound(arrColors, 2)
		sColor = Replace(arrColors(0, i), " ", "")
		sColor = Replace(sColor, "(", "")
		sColor = Replace(sColor, ")", "")
		arrPanes2 = Split(sColor, ",")
		r = 0
		If UBound(arrPanes2) <> iPanesCount Then Exit Function
		For nPane = 0 To iPanesCount
			r = r + ( CLng( arrPanes2(nPane) ) - CLng( arrPanes1(nPane) ) ) ^ 2
		Next

		'    //
		p = 0
		r1 = 0
		r2 = 0
		For nPane = 0 To iPanesCount
			r1 = r1 + CLng( arrPanes1(nPane) ) ^ 2
			r2 = r2 + CLng( arrPanes2(nPane) ) ^ 2
			p = p + CLng( arrPanes1(nPane) ) * CLng( arrPanes2(nPane) )
		Next
		delta = r1 - p * p / ( r2 + 1e-20 )

		r = r * 0.1 + delta * 0.9
		'end //

		If r < r_best Then
			best = i
			r_best = r
		End If
	Next
	FindNearColor = best
End Function


