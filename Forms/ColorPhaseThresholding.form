sub lstPhases_CurentColumnChange(nColumn)
	If nColumn = 0 Then lstPhases.SetCurentCol 2
end sub

Sub lstPhases_CurentRowChange(nRow)
	Dim bTemp
	iActivePhase = nRow
	If optMode.ItemChecked = 2 Then
		ActionState.TerminateCurrentAction
		SwitchPhase iActivePhase + 10
		ph_idx = find_phase( lstPhases.GetCellAsString(2, nRow) )
		If ph_idx >= 0  Then
			sColor = lstGlPhases.GetCellAsColorRGB(1, ph_idx)
		Else
			sColor = "(0,0,0)"
		End If
		Data.SetValue "\Phase\Phase" & CStr(iActivePhase + 1 + 10) & "Color", sColor
	End If

	If colPh.Exists(nRow) Then 
		sldWeight.PosLo = colPh.item(nRow).Weight
		btnClearWeight.Disabled = (sldWeight.PosLo = 0)
	End If

	Call set_btnRemovePhase
End Sub

Sub lstPhases_DataChange(nColumn, nRow)
	If nColumn = 2 Then
		Call set_range_color(nRow)
		bShowPhasesMode = -3
		If optMode.ItemChecked = 1 Then Call optMode_OnChangeCheck(1)
		If optMode.ItemChecked = 2 Then Call lstPhases_CurentRowChange(nRow)
		bClassifierModified = True
		Call set_btnRemovePhase
		Call CheckSelected
	ElseIf nColumn = 0 Then
		colPh.item(nRow).Color = lstPhases.GetCellAsColorRGB(nColumn, nRow)
		If optMode.ItemChecked = 0 Then ActionHelper.SegmColorPhases
		btnRestoreColors.Disabled = False
	ElseIf nColumn = 1 Then
		phN = find_phase( lstPhases.GetCellAsString(2, nRow) )
		If phN = -1 Then
			lstPhases.SetCellValueColorRGB nColumn, nRow, "(255, 255, 255)"
		Else
			lstGlPhases.SetCellValueColorRGB 1, phN, lstPhases.GetCellAsColorRGB(nColumn, nRow)
			Call lstGlPhases_DataChange(1, phN)
		End If
		lstPhases.SetCurentCol 2
	End If
End Sub

sub lstPhases_OnDblClick(nColumn, nRow)
	If nColumn = 3 Then
		iActivePhase = nRow
		If colPh.item(nRow).Weight = 0 Then Exit Sub
		colPh.item(nRow).Weight = 0
		sldWeight.PosLo = 0
		Call sldWeight_PosChanged()
	End If
end sub

