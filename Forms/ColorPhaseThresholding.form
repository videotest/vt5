Sub classifiers_list_initialise(sActiveClassFile)
	Dim fso
	Dim arrClassFiles()
	strClassesPath = Data.GetValue("\Paths\Classes\")
	sActiveClassFile = LCase( gl_Script.GetFileName( sActiveClassFile ) )
	sClassFileFromClasses = LCase( gl_Script.GetFileName( Data.GetValue( "\Classes\ClassFile\" ) ) )

	Set fso = CreateObject("Scripting.FileSystemObject")
	Set f1 = fso.GetFolder(strClassesPath)
	n = -1
	For Each objf In f1.Files
		sName = objf.Name
		If gl_Script.GetFileExt(sName) = "class" Then
			If gl_Script.ClassifierType(strClassesPath & sName) = "F" Then
				n = n + 1
				ReDim Preserve arrClassFiles( n )
				arrClassFiles(n) = gl_Script.GetFileName(sName)
			End If
		End If
	Next
	
	If n = -1 Then
		Redim arrClassFiles(0)
		arrClassFiles(0) = sDefault
		Call create_new_classifier( arrClassFiles(0) & ".class" )
	Else
		Call gl_Script.sort_1D_array(arrClassFiles, True)
	End If

	cmbClassifier.DeleteContents
	iActive = 0
	iActiveC = 0
	For i = 0 To UBound(arrClassFiles)
		cmbClassifier.InsertString -1, arrClassFiles(i)
		If LCase(arrClassFiles(i)) = sActiveClassFile Then iActive = i
		If LCase(arrClassFiles(i)) = sClassFileFromClasses Then iActiveC = i
	Next

	If iActiveC > 0 Then
		cmbClassifier.SetSel iActiveC
	Else
		cmbClassifier.SetSel iActive
	End If

	Set fso = Nothing

	Call read_classifier
End Sub

'===============================================================
'    //
'   "f.cls" //
'===============================================================
Sub create_new_classifier(sName)
	Dim fso
	strClassesPath = Data.GetValue("\Paths\Classes\")
	Set fso = CreateObject("Scripting.FileSystemObject")
	If fso.FileExists(strClassesPath & sName) Then
		fso.DeleteFile (strClassesPath & sName)
	End If
	If fso.FileExists(strClassesPath & "f.cls") Then
		fso.CopyFile strClassesPath & "f.cls", strClassesPath & sName, True
	Else
		Set f = fso.CreateTextFile (strClassesPath & sName, True)
		If TypeName(f) <> "Nothing" Then
			f.WriteLine "[General]"
			f.WriteLine "ClassifyProgID=ClassBase.ManualClassifier"
			f.WriteLine ""
			f.WriteLine "[Classes]"
			f.WriteLine "ClassCount=2"
			f.WriteLine "-1=(255,255,0),?"
			f.WriteLine "0=(255,0,0),1"
			f.WriteLine "1=(0,255,0),2"
			f.Close
			Set f = Nothing
		End If
	End If
	Set fso = Nothing
	iLastClassifier = -1
End Sub

'===============================================================
'     //
'===============================================================
Sub read_classifier
	strClassesPath = Data.GetValue("\Paths\Classes\")
	strClassFile = strClassesPath & cmbClassifier.GetText & ".class"
	
	edtPhasesN.ValueInt = CInt( Application.GetPrivateProfileString(strClassFile, "Classes", "ColorsCount", "5") )

	For i = 0 To lstGlPhases.GetRowCount - 1
		lstGlPhases.DeleteRow 0
	Next

	n = CInt( Application.GetPrivateProfileString(strClassFile, "Classes", "ClassCount", "0") )
	For i = 0 To n - 1
		sClassInfo = Application.GetPrivateProfileString _
		  (strClassFile, "Classes", CStr(i), "(0,0,0)," & sPhaseText & CStr(i))
		pos = InstrRev(sClassInfo, ",")
		If pos > 0 Then 
			sName = Mid(sClassInfo, pos + 1)
		Else
			sName = ""
		End If
		lstGlPhases.AddRow i
		lstGlPhases.SetCellValue 0, i, sName
		If pos > 0 Then 
			sColor = Left(sClassInfo, pos - 1)
		Else
			sColor = "(0,0,0)"
		End If
		lstGlPhases.SetCellValueColorRGB 1, i, sColor
		Data.SetValue "\Phase\Phase" & CStr(i + 1) & "Color", sColor
	Next
	Call lstGlPhases_CurentRowChange(0)
	btnRemovePhase.Disabled = ( lstGlPhases.GetRowCount < 3 )
	bClassifierModified = False
End Sub

'===============================================================
'   () //
'===============================================================
Sub phases_initialise
	Dim ph()
	Dim Attaching()

	n = lstGlPhases.GetRowCount
	ReDim ph(n-1)
	For i = 0 To n - 1
		ph(i) = lstGlPhases.GetCellAsString(0, i)
	Next

	If bClassifierModified Then
		ReDim Attaching(1, colPh.Count - 1)
		For i = 0 To Ubound(Attaching, 2)
			Attaching(0, i) = colPh.item(i).Color
			Attaching(1, i) = lstPhases.GetCellAsString (2, i)
		Next
	End If

	For i = 1 To lstPhases.GetRowCount - edtPhasesN.ValueInt
		lstPhases.DeleteRow lstPhases.GetRowCount - 1
	Next
	For i = 1 To edtPhasesN.ValueInt - lstPhases.GetRowCount
		lstPhases.AddRow lstPhases.GetRowCount
	Next

	colPh.RemoveAll
	For i = 0 To edtPhasesN.ValueInt - 1
		iActivePhase = i
		'lstPhases.AddRow i
		colPh.Add i, ""
		Set colPh.item(i) = New ColorPhase
		colPh.item(i).Name = CStr(i + 1)
		colPh.item(i).Color = Data.GetDefValue("\SegmColorPhases\Color" & CStr(i), "(0,0,0)")
		colPh.item(i).Weight = 0
		
		lstPhases.ClearCombo 2, i
		lstPhases.AddComboString 2, i, sIgnoreRange
		For j = 0 To n - 1
			lstPhases.AddComboString 2, i, ph(j)
		Next
		'lstPhases.SetCellValue 2, i, sIgnoreRange

		If bClassifierModified Then
			iFoundColor = FindNearColor( Attaching, colPh.item(i).Color )
			If iFoundColor > -1 Then
				lstPhases.SetCellValue 2, i, Attaching( 1, iFoundColor )
			Else
				lstPhases.SetCellValue 2, i, sIgnoreRange
			End If
			Call set_range_color(i)
		Else
			lstPhases.SetCellValue 2, i, sIgnoreRange
		End If

	Next

	Data.SetValue "\Phase\PhasesCount", colPh.Count
	lstPhases.SetCurentCol 2

	If Not bClassifierModified Then Call find_appropriate_phases

	Call CheckSelected
End Sub

'===============================================================
'         //
'===============================================================
Sub find_appropriate_phases
	Dim Attaching()
	strClassesPath = Data.GetValue("\Paths\Classes\")
	strClassFile = strClassesPath & cmbClassifier.GetText & ".class"
	n = CInt( Application.GetPrivateProfileString(strClassFile, "Classes", "ClassCount", "0") )
	ReDim Attaching(1, -1)
	k = 0
	For i = -1 To n - 1
		sColors = Application.GetPrivateProfileString (strClassFile, "AssignedColors", CStr(i), "")
		arrColors = Split(sColors, "+")
		For j = 0 To UBound(arrColors)
			ReDim Preserve Attaching(1, k)
			Attaching(0, k) = arrColors(j)
			Attaching(1, k) = i
			k = k + 1
		Next
	Next

	For i = 0 To edtPhasesN.ValueInt - 1
		iFoundColor = FindNearColor( Attaching, colPh.item(i).Color )
		If iFoundColor > -1 Then
			If Attaching( 1, iFoundColor ) > -1 Then
				sPhaseName = lstGlPhases.GetCellAsString(0, Attaching( 1, iFoundColor ))
			Else
				sPhaseName = sIgnoreRange
			End If
			lstPhases.SetCellValue 2, i, sPhaseName
		Else
			lstPhases.SetCellValue 2, i, sIgnoreRange
		End If
		Call set_range_color(i)
	Next
End Sub

