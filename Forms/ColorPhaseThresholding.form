sub cmbClassifier_OnSelectChange()
	Dim ph()
	
	If iLastClassifier = cmbClassifier.GetSel Then Exit Sub

	ActionState.TerminateCurrentAction
	
	If bClassifierModified Then
		iAnswer = MsgBox (Replace( sWriteClassifier, "XX", cmbClassifier.GetString(iLastClassifier) ), _
		  vbYesNoCancel + vbQuestion, gl_script.sVT)
		Select Case iAnswer
		  Case vbYes
			Call create_phases_classifier( cmbClassifier.GetString(iLastClassifier) )
		  Case vbCancel
			cmbClassifier.SetSel iLastClassifier
			Exit Sub
		End Select
	End If

	Call read_classifier

	Call phases_initialise

	If optMode.ItemChecked > 0 Then
		bShowPhasesMode = -2
		Call optMode_OnChangeCheck(optMode.ItemChecked)
	End If

	lstPhases.SetCurentRow 0
	Call lstPhases_CurentRowChange(0)
end sub

'***************************************************************
'btnNewClassifier event handlers.
'***************************************************************
sub btnNewClassifier_Click()
	Dim fso
	Set fso = CreateObject("Scripting.FileSystemObject")
	strClassesPath = Data.GetValue("\Paths\Classes\")

	strNameUser = Trim(InputBox(sInputClassifierName, gl_script.sVT))
	strName = Replace(LCase(strNameUser), ".class", "")
	If strName = "" Then Exit Sub
	If InStr(strName, "/") Or InStr(strName, "\") Or _
	  InStr(strName, "<") Or InStr(strName, ">") Or _
	  InStr(strName, """") Or InStr(strName, ":") Or _
	  InStr(strName, "*") Or InStr(strName, "?") Then
		MsgBox sWrongSymbol, vbExclamation, gl_Script.sVT
		Exit Sub
	End If
	strName = Left(strNameUser, Len(strName)) & ".class"

	If fso.FileExists (strClassesPath & strName) Then
		MsgBox sClassifierExists, vbExclamation, gl_Script.sVT
	Else
		Call create_new_classifier(strName)
		Call classifiers_list_initialise(strName)
		Call cmbClassifier_OnSelectChange
	End If
end sub



'***************************************************************
'optMode event handlers.
'***************************************************************
sub optMode_OnChangeCheck(lItem)
	Dim ph()
	ActionState.TerminateCurrentAction

	Data.SetValue "\Phase\PhasesCount", colPh.Count + 20

	Select Case lItem
	  Case 0

		SwitchPhase -1
		Call ShowPhases(0)

	  Case 1

		SwitchPhase -1
		Call ShowPhases(1)

	  Case 2

		SwitchPhase iActivePhase + 10
		Call ShowPhases(0)
		Call lstPhases_CurentRowChange(lstPhases.GetCurentRow)

	End Select
end sub

'===============================================================
'     (1) /  (0) //
'===============================================================
sub ShowPhases(bShow)
	Dim ph()

	If bShow Then
		n = lstGlPhases.GetRowCount

		ReDim ph(1, n - 1)
		For i = 0 To n - 1
			ph(0, i) = lstGlPhases.GetCellAsString(0, i)
			ph(1, i) = lstGlPhases.GetCellAsColorRGB(1, i)
		Next

		For i = 0 To colPh.Count - 1
			
			sName = lstPhases.GetCellAsString(2, i)
			If sName <> sIgnoreRange Then
				j = n
				Do While j > 0
					j = j - 1
					If sName = ph(0, j) Then Exit Do
				Loop
				Data.SetValue "\SegmColorPhases\Phase" & CStr(i), 128 + j
			Else
				Data.SetValue "\SegmColorPhases\Phase" & CStr(i), 0
			End If
			
		Next
	Else
		For i = 0 To colPh.Count - 1
			Data.SetValue "\Phase\Phase" & CStr(i + 1 + 10) & "Color", colPh.item(i).Color
			Data.SetValue "\SegmColorPhases\Phase" & CStr(i), 128 + i + 10
		Next
	End If

	If Not bFormDestroy And bShowPhasesMode <> bShow Then
		If bShowPhasesMode <> -3 Then Form.CancelPreview
		bShowPhasesMode = bShow
		ActionHelper.SegmColorPhases
	End If
end sub

'===============================================================
'         //
'===============================================================
Function find_phase(sName)
	find_phase = -1
	For i = 0 To lstGlPhases.GetRowCount - 1
		If lstGlPhases.GetCellAsString(0, i) = sName Then
			find_phase = i
			Exit Function
		End If
	Next
End Function


